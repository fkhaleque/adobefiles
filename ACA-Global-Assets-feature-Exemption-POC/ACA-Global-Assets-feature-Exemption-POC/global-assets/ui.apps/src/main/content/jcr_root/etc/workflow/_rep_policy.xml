<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow-workflow-users
        jcr:primaryType="rep:GrantACE"
        rep:principalName="workflow-users"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,rep:write,jcr:lockManagement]"/>
    <allow-aca-service
        jcr:primaryType="rep:GrantACE"
        rep:principalName="aca-service"
        rep:privileges="{Name}[jcr:read,jcr:versionManagement,crx:replicate,rep:write,jcr:lockManagement]"/>
</jcr:root>
