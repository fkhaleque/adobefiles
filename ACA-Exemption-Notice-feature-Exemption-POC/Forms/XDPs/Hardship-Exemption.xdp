         <script contentType="application/x-javascript" name="soExemption" runAt="both">
/******************************************************************
 *
 *  soExemption
 *  
 *  Form-specific logic and validation functions
 *  
 ******************************************************************/

/* GLOBAL instance-specific varables */

var FORM_NAME = "Application for Exemption from the Shared Responsibility Payment for Individuals who experience Hardships";
var BC_FORM_NAME = "2015 EXEMPTION HARDSHIP APP";
var PERFORM_SECTIONAL_VALIDATION = true;
var PERFORM_SECTIONAL_VALIDATION_ENFORCE_FILL = true;


/**
 * Calls all section validation functions and validates form
 *   @return - If form is valid, returns true; otherwise, false
 */
function validateFormSections()
{
	//Note: not resetting error fields here; reset by global or by one-off calls to this function.

	//Validate Step 1 - Personal Information
	validateStep1(xfa.resolveNode("pgStep1.sfStep1[0]"));
	
	//Validate Step 2 - First person
	validateStep2Person(xfa.resolveNode("pgStep2Person1.sfPerson1[0]"), 1);

	//Validate Step 2 if pgStep2Person2 is visible - All Person 2s
	var intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
	var objPage = xfa.resolveNode("pgStep2Person2");	

	if ( intStep2PeopleCount &gt; 0 &amp;&amp; objPage.presence != "hidden" )
	{	
		for( var i=0; i&lt;intStep2PeopleCount; i++ )
		{
			objNode = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s[" + i + "]");
			//app.alert("objNode: " + objNode.somExpression);
			validateStep2Person(objNode, i + 2);
		}
	}

	//Validate Step 3 - Read &amp; Sign This Application		
	validateStep3(xfa.resolveNode("pgStep3and4[0]"));

	//Validate Appendix C agent, nav, counselor and broker assistance section of form
	validateAppendixC_CounselorAssist(xfa.resolveNode("pgAppendixC[0]"));
	
	//Validate Appendix C authorized representative section of form
	validateAppendixC_AuthorizedRepAssist(xfa.resolveNode("pgAppendixC[0]"));
	
	soValidations.isValid();
}


/**
 * Validates Step 1 page of form
 *  @param - objNode - form node to evaluate
 *  @return - If valid, returns true; otherwise, false
 */
function validateStep1(objNode)
{	
	/*
	 * Check for first and last names
	 */	
	if ( !soValidations.requiredFieldCheck(objNode.sfLegalName.fldFirstName, "") )
	{	
		soValidations.addError(objNode.sfLegalName.fldFirstName, "STEP 1", "- First name is empty");
	}
	
	if ( !soValidations.requiredFieldCheck(objNode.sfLegalName.fldLastName, "") ) 
	{
		soValidations.addError(objNode.sfLegalName.fldLastName, "STEP 1", "- Last name is empty");
	}


	/*
	 * Check for valid addresses
	 */
	var homeEmpty = soValidations.areTheseFieldsEmpty([objNode.sfHomeAddress.fldStreetAddress, objNode.sfHomeAddress.fldAptNo,
													objNode.sfHomeAddress.fldCity, objNode.sfHomeAddress.fldState,
													objNode.sfHomeAddress.fldZip, objNode.sfHomeAddress.fldCounty]);
	
	var mailEmpty = soValidations.areTheseFieldsEmpty([objNode.sfHomeAddress.fldStreetAddress, objNode.sfHomeAddress.fldAptNo,
													objNode.sfHomeAddress.fldCity, objNode.sfHomeAddress.fldState,
													objNode.sfHomeAddress.fldZip, objNode.sfHomeAddress.fldCounty]);
	
	var homeValid = soValidations.isAddressValid(objNode.sfHomeAddress.fldStreetAddress.rawValue,
													objNode.sfHomeAddress.fldAptNo.rawValue,
													objNode.sfHomeAddress.fldCity.rawValue,
													objNode.sfHomeAddress.fldState.rawValue,
													objNode.sfHomeAddress.fldZip.rawValue,
													objNode.sfHomeAddress.fldCounty.rawValue,
													false);
	
	var mailValid =  soValidations.isAddressValid(objNode.sfMailingAddress.fldStreetAddress.rawValue, 
														objNode.sfMailingAddress.fldAptNo.rawValue,
														objNode.sfMailingAddress.fldCity.rawValue,
														objNode.sfMailingAddress.fldState.rawValue,
														objNode.sfMailingAddress.fldZip.rawValue,
														objNode.sfMailingAddress.fldCounty.rawValue,
														false);
	
	//Check if both are invalid
	if ( !homeValid &amp;&amp; !mailValid )
	{
		//At least one of them must be specified.
		//Re-run the validations, this time, highlighting the broken fields
		soValidations.isValidAddress(objNode.sfHomeAddress.fldStreetAddress, 
									objNode.sfHomeAddress.fldAptNo,
									objNode.sfHomeAddress.fldCity,
									objNode.sfHomeAddress.fldState,
									objNode.sfHomeAddress.fldZip,
									objNode.sfHomeAddress.fldCounty,
									true);
	
		soValidations.isValidAddress(objNode.sfMailingAddress.fldStreetAddress, 
									objNode.sfMailingAddress.fldAptNo,
									objNode.sfMailingAddress.fldCity,
									objNode.sfMailingAddress.fldState,
									objNode.sfMailingAddress.fldZip,
									objNode.sfMailingAddress.fldCounty,
									true);
									
		soValidations.addError(objNode.sfHomeAddress.fldStreetAddress, "STEP 1", "- Must specify a valid home or mailing address");
	
	}

	
	/*
	 * Check for at least one valid phone number
	 */
	var emptyPhone = soValidations.areTheseFieldsEmpty([objNode.sfPhoneNumbers.fldPhoneArea, objNode.sfPhoneNumbers.fldPhonePrefix, objNode.sfPhoneNumbers.fldPhoneSuffix]);
	var emptyPhoneOther = soValidations.areTheseFieldsEmpty([objNode.sfPhoneNumbers.fldPhoneOtherArea, objNode.sfPhoneNumbers.fldPhoneOtherPrefix, objNode.sfPhoneNumbers.fldPhoneOtherSuffix]);
	var errPhone = false;
	var errPhoneOther = false;
	
	if ( emptyPhone &amp;&amp; emptyPhoneOther )
	{
		//No phone number are filled
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneArea, "STEP 1", "- Must specify at least one valid telephone number");
		errPhone = true;
		errPhoneOther = true;
	}
	else
	{	
		var validPhone = soValidations.concatAndValidatePhone(objNode.sfPhoneNumbers.fldPhoneArea, objNode.sfPhoneNumbers.fldPhonePrefix, objNode.sfPhoneNumbers.fldPhoneSuffix);
		var validPhoneOther = soValidations.concatAndValidatePhone(objNode.sfPhoneNumbers.fldPhoneOtherArea, objNode.sfPhoneNumbers.fldPhoneOtherPrefix, objNode.sfPhoneNumbers.fldPhoneOtherSuffix);	
		
		if ( false == ( ( validPhone &amp;&amp; validPhoneOther ) || ( validPhone &amp;&amp; emptyPhoneOther ) || ( emptyPhone &amp;&amp; validPhoneOther ) ) )
		{
			//Filled, but it's not valid
			if ( !validPhone &amp;&amp; emptyPhoneOther )
			{
				//Phone is invalid, but Other Phone is not specified
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneArea, "STEP 1", "- Phone number is invalid");	
				errPhone = true;
			}
			else if ( !validPhoneOther &amp;&amp; emptyPhone )
			{
				//Other Phone is is invalid, but Phone is not specified
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherArea, "STEP 1", "- Other phone number is invalid");	
				errPhoneOther = true;
			}
			else
			{
				//Both are invalid
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneArea, "STEP 1", "- Phone number is invalid");	
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherArea, "STEP 1", "- Other phone number is invalid");
				errPhone = true;
				errPhoneOther = true;
			}
		}
	}
	
	if ( errPhone )
	{
		soValidations.addError(objNode.sfPhoneNumbers.fldPhonePrefix, "STEP 1", null);	
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneSuffix, "STEP 1", null);	
	}
	
	if ( errPhoneOther )
	{
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherPrefix, "STEP 1", null);	
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherSuffix, "STEP 1", null);		
	}
		
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneArea, errPhone);
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhonePrefix, errPhone);	
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneSuffix, errPhone);
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneOtherArea, errPhoneOther);
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneOtherPrefix, errPhoneOther);	
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneOtherSuffix, errPhoneOther);


	/*
	 * Check for valid e-mail
	 */
	var validEmail = soValidations.isValidEmail(objNode.sfEmailAddress.fldEmailAddress.rawValue) 
					| ( objNode.sfMarketplaceInfo.rdoMarketplaceInfo.rawValue != "Y" &amp;&amp; objNode.sfEmailAddress.fldEmailAddress.rawValue == null );
	
	soValidations.setFieldErrorState (objNode.sfEmailAddress.fldEmailAddress, !validEmail);
		
	if ( !validEmail )
	{
		soValidations.addError(objNode.sfEmailAddress.fldEmailAddress, "STEP 1", "- Email address is invalid");
	}

		
	return soValidations.isValid();

}


/**
 * Validates Step 2 pages for all people
 *  @param - objNode - form node to evaluate
 *  @param - intPerson - person number to validate
 *  @return - If valid, returns true; otherwise, false
 */
function validateStep2Person(objNode, intPerson)
{
	//Process Person1 AND Person2s nodes only if pgStep2Person2 page is visible
	if ( intPerson == 1 || ( intPerson &gt; 1 &amp;&amp; objNode.parent.parent.presence != "hidden" ) )
	{
		//Clear error states
		soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBYear, false);
		soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBDay, false);	
		soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBMonth, false);
		soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN1, false);
		soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN2, false);
		soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN3, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.rdoPlanToFile, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.rdoFileJointly, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.fldSpouse, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.flddependents, false);
		soValidations.setFieldErrorState(objNode.sfWantExemption.rdoWantExemption, false);
	
		//Person 1 has a list box for number of dependents while Person &gt;1 has a yes / no checkbox
		if ( intPerson == 1 )
			soValidations.setFieldErrorState(objNode.sfTaxes.listDependents, false);
		else
			soValidations.setFieldErrorState(objNode.sfTaxes.rdoClaimdependents, false);
	
		//Check if Person 2 first name is filled if 6a in Step 2 is yes (spouse name is required)
		if ( intPerson == 2 )
		{
			if ( pgStep2Person1.sfPerson1.sfTaxes.rdoFileJointly.rawValue == "Y" &amp;&amp;
					( objNode.sfPerson.sfName.fldFirstName.rawValue == "" || objNode.sfPerson.sfName.fldFirstName.rawValue == null ) )
			{
				soValidations.addError(objNode.sfPerson.sfName.fldFirstName, "STEP 2 - PERSON " + intPerson, "- Spouse's first name is required if filing jointly");
				soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldFirstName, true);
			}
		}
		
		if ( intPerson &gt; 1 )
		{
			//Name for intPerson = 1 is mirrored from Step 1 into a locked field
			
			if ( !soValidations.requiredFieldCheck(objNode.sfPerson.sfName.fldFirstName, "") )
			{	
				soValidations.addError(objNode.sfPerson.sfName.fldFirstName, "STEP 2 - PERSON " + intPerson, "- First name is empty");
				soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldFirstName, true);
			}
			
			if ( !soValidations.requiredFieldCheck(objNode.sfPerson.sfName.fldLastName, "") ) 
			{
				soValidations.addError(objNode.sfPerson.sfName.fldLastName,  "STEP 2 - PERSON " + intPerson, "- Last name is empty");
				soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldLastName, true);		
			}
		}
						
		//Check for validity of DOB or SSN
		var validDOB = soValidations.isValidDate(objNode.sfPerson.sfName.fldDOBYear.rawValue, objNode.sfPerson.sfName.fldDOBMonth.rawValue, objNode.sfPerson.sfName.fldDOBDay.rawValue);
		var validSSN = soValidations.isValidSSN(objNode.sfPerson.sfSSN.fldSSN1.editValue + objNode.sfPerson.sfSSN.fldSSN2.editValue + objNode.sfPerson.sfSSN.fldSSN3.editValue);
	
		if ( !validDOB )
		{	
			soValidations.addError(objNode.sfPerson.sfName.fldDOBMonth, "STEP 2 - PERSON " + intPerson, "- Date of Birth is not valid");
			soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBYear, true);
			soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBDay, true);	
			soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBMonth, true);
		}
		
		//SS# is no longer required
		/*
		if ( !validSSN )
		{	
			soValidations.addError(objNode.sfPerson.sfSSN.fldSSN1, "STEP 2 - PERSON " + intPerson, "- Social Security Number is not valid");
			soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN1, true);
			soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN2, true);
			soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN3, true);
		}
		*/
		
		if ( !validDOB &amp;&amp; !validSSN )   //At least one must be valid
		{
			soValidations.addError(objNode.sfPerson.sfName.fldDOBYear, "STEP 2 - PERSON " + intPerson, "- Valid Date of Birth or Social Security Number must be specified");
			soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBYear, true);
			soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBDay, true);	
			soValidations.setFieldErrorState(objNode.sfPerson.sfName.fldDOBMonth, true);
			soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN1, true);
			soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN2, true);
			soValidations.setFieldErrorState(objNode.sfPerson.sfSSN.fldSSN3, true);
		}
	
		// planning to file?
		switch (objNode.sfTaxes.rdoPlanToFile.rawValue)
		{
			case null:  //Did not specify
			case "":
				soValidations.addError(objNode.sfTaxes.rdoPlanToFile, "STEP 2 - PERSON " + intPerson, "- Must indicate if person plans to file a federal income tax return");
				soValidations.setFieldErrorState(objNode.sfTaxes.rdoPlanToFile, true);
				break;
			
			case "Y":  //Yes is filing
		
				switch (objNode.sfTaxes.rdoFileJointly.rawValue)
				{
					case null: //Must specify jointly
					case "":
						soValidations.addError(objNode.sfTaxes.rdoFileJointly, "STEP 2 - PERSON " + intPerson, "- Must indicate if you plan to file jointly with a spouse");
			    		soValidations.setFieldErrorState(objNode.sfTaxes.rdoFileJointly, true);
			    		break;
			    		
			    	case "Y": //Has spouse
	
			    		//Check for spouse name
			    		if ( !soValidations.requiredFieldCheck(objNode.sfTaxes.fldSpouse, "") ) 
			    		{		    		
			    			soValidations.addError(objNode.sfTaxes.fldSpouse, "STEP 2 - PERSON " + intPerson, "- Spouse's name is missing");
			    			soValidations.setFieldErrorState(objNode.sfTaxes.fldSpouse, true);
			    		}
			    		break;
			    }
			    
			    //Person 1 has a list box for number of dependents while Person &gt;1 has a yes / no checkbox
			    if ( intPerson == 1 )
			    {
					switch (objNode.sfTaxes.listDependents.rawValue)
					{
						case null: //Must specify number of dependents
						case "":
							soValidations.addError(objNode.sfTaxes.listDependents, "STEP 2 - PERSON " + intPerson, "- Must indicate if you will claim any dependents and how many");
				    		soValidations.setFieldErrorState(objNode.sfTaxes.listDependents, true);
				    		break;
						
						case "0":  //No dependents
				    		break;			    		
				    		
				    	default: //Has dependents
	
				    		//Check for dependent names
				    		if ( !soValidations.requiredFieldCheck(objNode.sfTaxes.flddependents, "") ) 
				    		{
				    			soValidations.addError(objNode.sfTaxes.flddependents, "STEP 2 - PERSON " + intPerson, "- Dependent name(s) are missing");
				    			soValidations.setFieldErrorState(objNode.sfTaxes.flddependents, true);
				    		}
				    		break;
					}
			    }
			    else
			    {
					switch (objNode.sfTaxes.rdoClaimdependents.rawValue)
					{
						case null: //Must specify claimed dependents
						case "":
							soValidations.addError(objNode.sfTaxes.rdoClaimdependents, "STEP 2 - PERSON " + intPerson, "- Must indicate if you will claim any dependents");
				    		soValidations.setFieldErrorState(objNode.sfTaxes.rdoClaimdependents, true);
				    		break;
				    		
				    	case "Y": //Has dependents
				    		
				    		//Check for dependent names
				    		if (!soValidations.requiredFieldCheck(objNode.sfTaxes.flddependents, "")) 
				    		{
				    			soValidations.addError(objNode.sfTaxes.flddependents, "STEP 2 - PERSON " + intPerson, "- Dependent name(s) are missing");
				    			soValidations.setFieldErrorState(objNode.sfTaxes.flddependents, true);
				    		}
				    		break;
					}
				}
		}
	
		if ( objNode.sfWantExemption.rdoWantExemption.rawValue == "" )
		{
			soValidations.addError(objNode.sfWantExemption.rdoWantExemption, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No whether you want the exemption");
			soValidations.setFieldErrorState(objNode.sfWantExemption.rdoWantExemption, true);
		} 

		if ( intPerson &gt; 1 )
		{
			if ( !soValidations.requiredFieldCheck(objNode.sfTaxes.rdoOnP1Taxes, "") )
			{
				soValidations.addError(objNode.sfTaxes.rdoOnP1Taxes, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No whether you'll be claimed as a dependent for Person 1");
				soValidations.setFieldErrorState(objNode.sfTaxes.rdoOnP1Taxes, true);
			}
			else if ( objNode.sfTaxes.rdoOnP1Taxes.rawValue == "Y" )
			{
				if ( !soValidations.requiredFieldCheck(objNode.sfTaxes.fldFiler, "") )
				{
					soValidations.addError(objNode.sfTaxes.fldFiler, "STEP 2 - PERSON " + intPerson, "- Must indicate name of tax filer");
					soValidations.setFieldErrorState(objNode.sfTaxes.fldFiler, true);
				}
				
				if ( !soValidations.requiredFieldCheck(objNode.sfTaxes.fldRelToFiler, "") )
				{
					soValidations.addError(objNode.sfTaxes.fldRelToFiler, "STEP 2 - PERSON " + intPerson, "- Must indicate relation to tax filer");
					soValidations.setFieldErrorState(objNode.sfTaxes.fldRelToFiler, true);
				}
			}
		}
		
		//Validate at least two pages of STEP 2 exist for both spouse and dependent
		if ( intPerson == 1 )
		{
			var intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
			
			if ( intStep2PeopleCount &lt; 2 &amp;&amp;
					objNode.sfTaxes.rdoFileJointly.rawValue == "Y" &amp;&amp;
						objNode.sfTaxes.listDependents.rawValue &gt;= 1 )
			{
				soValidations.addError(objNode.sfTaxes.rdoPlanToFile, "STEP 2 - PERSON " + intPerson, "- At least 2 people in Step 2 need exist for both spouse &amp; dependent");
			}
		}
		
		//Validate hardship grid
		var intCountHS = validateHardships(objNode, "STEP 2 - PERSON " + intPerson);
		//app.alert("intCountHS: " + intCountHS);
		
		//Check hardship counts
		if ( intCountHS &gt; 0 &amp;&amp; ( objNode.sfWantExemption.rdoWantExemption.rawValue == "" ||
									objNode.sfWantExemption.rdoWantExemption.rawValue == null || 
										objNode.sfWantExemption.rdoWantExemption.rawValue == "N" ) )
		{
			soValidations.addError(objNode.sfWantExemption.rdoWantExemption, "STEP 2 - PERSON " + intPerson, "- This exemption is not specified/desired, but hardship(s) were indicated");
			soValidations.setFieldErrorState(objNode.sfWantExemption.rdoWantExemption, true);
		}
		else if ( intCountHS == 0 &amp;&amp; objNode.sfWantExemption.rdoWantExemption.rawValue == "Y" )
		{
			soValidations.addError(objNode.sfWantExemption.rdoWantExemption, "STEP 2 - PERSON " + intPerson, "- This exemption is desired, but no hardship(s) were indicated");
			soValidations.setFieldErrorState(objNode.sfWantExemption.rdoWantExemption, true);
		}
	}
	
	//Were there issues?
	return soValidations.isValid();

}


/**
 * Validates hardships section
 *  @param - objNode - object node to evaluate
 *  @param - strSect - section in which hardship is being evaluated
 *  @return - If valid, returns true; otherwise, false
 */
function validateHardships(objNode, strSect)
{
	//pgStep2Person1.sfPerson1.sfHardships._sfHardship
	var vItems = objNode.sfHardships.resolveNodes("sfHardship[*]");
	var intCountHS = 0;

	//Clear error states - attestation	
	soValidations.setFieldErrorState(objNode.sfHardships.sfAttestation.fldAttestation, false);	
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		var startDate=null;
		var endDate =null;
	
		//console.println(vItems.item(i).name + " " + i);

		//Clear error states - hardships	
		soValidations.setFieldErrorState(vItems.item(i).fldHSTaxYear, false);
		soValidations.setFieldErrorState(vItems.item(i).fldHSDateStart, false);
		soValidations.setFieldErrorState(vItems.item(i).fldHSDateEnd, false);
			
		if ( vItems.item(i).fldHSDateStart.rawValue != null )
		{
			startDate = new Date(vItems.item(i).fldHSDateStart.rawValue.replace(/-/g, "/"));
		}
		if ( vItems.item(i).fldHSDateEnd.rawValue != null )
		{
			endDate = new Date(vItems.item(i).fldHSDateEnd.rawValue.replace(/-/g, "/"));
		}

		// Wants the exemption?
		if ( vItems.item(i).fldHS.rawValue &gt; 0 )
		{	
			//Increment hardship count
			intCountHS++;
		
			// dec 03 aft05601
			// CMS hasn't figured out how to properly do tax year rules
			// as a time period may actually span two tax years.
			// too late to back this field out of the design, so we've made this field optional
			// 		
			//if ( !soValidations.isValidYear("20" + vItems.item(i).fldHSTaxYear.editValue) )
		    //	{
			//	soValidations.addError(vItems.item(i).fldHSTaxYear, strSect, "- Taxation year is required for hardship number " + (i+1));
			//	soValidations.setFieldErrorState(vItems.item(i).fldHSTaxYear, true);
			// }
			
			if ( startDate == null )
			{
				soValidations.addError(vItems.item(i).fldHSDateStart, strSect, "- Start date is required for hardship number " + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).fldHSDateStart, true);
			}
			else if ( startDate &lt; 0 )
			{
				soValidations.addError(vItems.item(i).fldHSDateStart, strSect, "- Start date is required for hardship number " + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).fldHSDateStart, true);
			}
			
			if ( vItems.item(i).fldHSOngoing.rawValue == "Y" )
			{
				// ongoing.  only valid end date is empty.
				if ( endDate != null )
				{
					soValidations.addError(vItems.item(i).fldHSDateEnd, strSect, "- Both an end date and 'ongoing' has been specified for hardship number " + (i+1) + " - Select one option only");
					soValidations.setFieldErrorState(vItems.item(i).fldHSDateEnd, true);
				} 
			}
			else
			{
				if ( endDate == null )
				{
					soValidations.addError(vItems.item(i).fldHSDateEnd, strSect, "- End date is required for hardship number " + (i+1));
					soValidations.setFieldErrorState(vItems.item(i).fldHSDateEnd, true);
				}
				else if ( endDate &lt; 0 )
				{
					soValidations.addError(vItems.item(i).fldHSDateEnd, strSect, "- Valid end date is required for hardship number " + (i+1));
					soValidations.setFieldErrorState(vItems.item(i).fldHSDateEnd, true);
				}
				else
				{
					// do date compare if start date is also specified
					if ( startDate != null )
					{
						if ( startDate.getTime() == endDate.getTime() )
						{
							// dates are same
							soValidations.addError(vItems.item(i).fldHSDateEnd, strSect, "- Start date and end date for hardship number " + (i+1) + " are the same");
							soValidations.setFieldErrorState(vItems.item(i).fldHSDateEnd,true);
						}
						else if ( ( startDate.getTime() - endDate.getTime() ) &gt; 0 )
						{
							// dates are out of order
							soValidations.addError(vItems.item(i).fldHSDateEnd, strSect, "- End date for hardship number " + (i+1) + " is before the start date");
							soValidations.setFieldErrorState(vItems.item(i).fldHSDateEnd, true);
						}
					}
			
				}
			}

			//Check dates for included tax year
			if ( ! validateHardshipDatesIncludeTaxYear(vItems.item(i).fldHSTaxYear, vItems.item(i).fldHSDateStart, vItems.item(i).fldHSDateEnd) )
			{
				soValidations.addError(vItems.item(i).fldHSTaxYear, strSect, "- Tax year for hardship number " + (i+1) + " is not within any specified date");
				soValidations.setFieldErrorState(vItems.item(i).fldHSTaxYear, true);
			}
	
			//Check for attestation for other hardship
			if ( i == 13 &amp;&amp; ( objNode.sfHardships.sfAttestation.fldAttestation.rawValue == "" || objNode.sfHardships.sfAttestation.fldAttestation.rawValue == null ) )
			{
				// no description for other hardship
				soValidations.addError(objNode.sfHardships.sfAttestation.fldAttestation, strSect, "- Description for hardship number " + (i+1) + " cannot be blank");
				soValidations.setFieldErrorState(objNode.sfHardships.sfAttestation.fldAttestation, true);
			}
			
		}
		else
		{
			// Does not want exemption.
			// it won't be included in barcode
		}
	}
	
	return intCountHS;
}


