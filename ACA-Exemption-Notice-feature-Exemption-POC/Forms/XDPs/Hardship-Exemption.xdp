		vItems.item(i).fldHSDateStart.rawValue = null;
		vItems.item(i).fldHSDateEnd.rawValue = null;
		vItems.item(i).fldHSOngoing.rawValue = null;
	}
}


function initializeHardshipGrid(sfHardships)
{
	var vItems = sfHardships.resolveNodes("sfHardship[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		//console.println("initHardGrid: " + vItems.item(i).name + " " + i);
