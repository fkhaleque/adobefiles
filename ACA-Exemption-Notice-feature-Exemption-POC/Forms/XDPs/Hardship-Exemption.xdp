         <script contentType="application/x-javascript" name="soValidations" runAt="both">
/******************************************************************
 *
 *  soValidations
 *  
 *  Reusable valdition functions for common field types
 *  
 ******************************************************************/

/* GLOBAL alert variables */
var SHOW_ALERTS_DEFAULT = true;
var SHOW_ALERTS = true;

/* GLOBAL color variables */
var FIELD_BG_COLOUR_ERR = "225,0,0";
var FIELD_BG_COLOUR_OK = "255,255,255";
var FIELD_BORDER_COLOUR_ERR = "255,0,0";

/* GLOBAL error tracking variables */
var gIS_VALID = false;
var gERR_STR = "";
var gERR_SECTION = "";
var gERR_FIELDS = null;

var IS_EMPTY=0;
var IS_INVALID=1;
var IS_VALID=2;

var IS_OPTIONAL=0;
var IS_MANDATORY=1;

/* GLOBAL address type variables */
var ADDR_TYPE_HOME = "home";
var ADDR_TYPE_MAILING = "mailing";


/**
 * Calls global validations and then instance validations.
 *   @return - If form is valid, returns true; otherwise, false
 */
function validateForm()
{
	// Reset any previous errors
	resetErrors();
	
	// Validate common fields for all instance types
	// none yet...
	
	// Call the instance-specific section validations
	soExemption.validateFormSections();
	return soValidations.isValid();

}


/**
 * Sets error handling flags and messages to default values
 */
function resetErrors()
{
	// reset any fields
	if ( gERR_FIELDS != null )
	{
		for (var i=0; i&lt;gERR_FIELDS.length; i++) 
		{
			setFieldErrorState(gERR_FIELDS[i], false); 
		}
	}
	
	// Reset strings
	gERR_STR = "";
	gERR_SECTION = "";
	gERR_FIELDS = new Array();
	gIS_VALID = true;
}


/**
 * Records an error, adding the field to the list of errored fields
 * and appends a message to the error string.
 *  @param - fld - Field that errored
 *  @param - msg - Message (optional, may be null)
 */
function addError(fld, sect, msg)
{	
	if ( gERR_FIELDS == null )
	{
		gERR_FIELDS = new Array();
	}
	
	if ( gERR_STR == null )
	{
		gERR_STR = "";
	}
	
	if ( msg != null )
	{
		if ( gERR_SECTION == "" )
		{
			gERR_SECTION = sect;
			gERR_STR = sect + "\n";
		}
		else if ( gERR_SECTION != sect )
		{
			gERR_SECTION = sect;
			gERR_STR += "\n" + sect + "\n";
		}
		
		gERR_STR += msg + "\n";
	}
	
	gERR_FIELDS.push(fld);
	gIS_VALID = false;
}


/**
 * Displays a dialog box with any error messages that have accumulated.
 * If there are no errors, no box is shown.
 */
function errorMessageBox()
{
	if (!isValid())
	{
		xfa.host.messageBox(getErrorMessage(), soExemption.FORM_NAME, 1,0);
	}
}


/**
 * Returns validity of the form
 *  @return - Form is valid (true) or not (false)
 */
function isValid()
{
	return gIS_VALID;
}


/**
 * Gets accumulated error string
 *  @return - Error string
 */
function getErrorMessage()
{
	return gERR_STR;
}


/**
 * Highlights first field with error
 */
function highlightFirstErrorField()
{
	try
	{
		xfa.host.setFocus(gERR_FIELDS[0]);
	}
	catch (oErr)
	{
		xfa.host.messageBox("Can't focus"  + oErr);
	}	
}


/**
 * Checks field for null value
 *  @param - field - Field object to check
 *  @param - errmsg - Error message to display
 *  @return - Returns true if no error, false if error
 */
function requiredFieldCheck(field, errmsg)
{

	if ( field.rawValue == "" || field.rawValue == null )
	{	
		setFieldErrorState(field,true);
		// Are we showing errors, and do we have a message to show?
		if ( SHOW_ALERTS &amp;&amp; errmsg != null &amp;&amp; errmsg.length &gt; 0 )
		{
			xfa.host.messageBox(errmsg,soExemption.FORM_NAME, 1, 0);
		}
		
		// a problem.  return false.
		return false;
	}
	else
	{
		// unset error treatment
		setFieldErrorState(field, false);
		return true;
	}
}


/**
 * Un/Set the visual error state of a field background, borders, etc.
 *  @param - field - Field object to check
 *  @param - isError - Has error (true) otherwise (false)
 */
function setFieldErrorState(field, isError)
{
	//console.println(field.name + "/" + field.className + "/" + isError);

	if ( isError )
	{
		field.border.fill.presence = "visible";
		field.fillColor = FIELD_BG_COLOUR_ERR;
	}
	else
	{
		// radio buttons and check box fill need to go back to invisibility instead of white.
		if ( field.className == "exclGroup" )
		{
			field.border.fill.presence = "invisible";
		}
		else
		{ 
			field.fillColor = FIELD_BG_COLOUR_OK;
		}
	}
}


/**
 * Checks array of fields and determines if one is empty.
 *  @param - arrayOfFields - array of fields
 *  @return - if not (true) otherwise (false)
 */
function areTheseFieldsEmpty(arrayOfFields)
{
	for ( var i=0; i&lt;arrayOfFields.length; i++ )
	{
		if ( arrayOfFields[i].rawValue!=null )
		{
			return false;
		}
	}
	return true;
}


/**
 * Concatenates phone nubmer field parts
 *  @param - area - area code
 *  @param - prefix - phone prefix
 *  @param - suffix - phone suffix
 *  @return - concatenate phone number string
 */
function concatAndValidatePhone(area, prefix, suffix)
{
	var phoneNumber = area.formattedValue + prefix.formattedValue + suffix.formattedValue;
	
	return isValidPhone(phoneNumber);
}


/**
 * Checks for certain conditions with SSN that are not valid
 * and displays message and clears field if not valid 
 *  @param - field - field ID (SSN1 | SSN2 | SSN3)
 *  @param - objSSNPart - SSN part field object
 */
function validateSSNEntry(field, objSSNPart)
{
	if ( objSSNPart.rawValue != "" &amp;&amp; objSSNPart.rawValue != null )
	{
		if ( field == "SSN1" )
		{
			if ( objSSNPart.rawValue.charAt(0) == "9" )
			{
				xfa.host.messageBox("The first digit of SSN cannot cannot start with '9'.", soExemption.FORM_NAME,1,0);
		    	objSSNPart.rawValue = "";
			}
			else
			{	
				switch (objSSNPart.rawValue)
				{
					case "000":
					
						xfa.host.messageBox("The first 3 digits of SSN cannot be '000'.", soExemption.FORM_NAME,1,0);
		    			objSSNPart.rawValue = "";
		    			break;
					
					case "666":
					
						xfa.host.messageBox("The first 3 digits of SSN cannot be '666'.", soExemption.FORM_NAME,1,0);
		    			objSSNPart.rawValue = "";
		    			break;
				}
			}
		} 
		else if ( field == "SSN2" )
		{
			switch (objSSNPart.rawValue)
			{
				case "00":
				
					xfa.host.messageBox("The middle 2 digits of SSN cannot be '00'.", soExemption.FORM_NAME,1,0);
	    			objSSNPart.rawValue = "";
	    			break;
			}
		}  
		else if ( field == "SSN3" )
		{
			if ( objSSNPart.rawValue.substring(0,2) == "00" )
			{
				xfa.host.messageBox("The last 4 digits of SSN cannot begin with '00'.", soExemption.FORM_NAME,1,0);
		    	objSSNPart.rawValue = "";
			}
			else
			{	
				switch (objSSNPart.rawValue)
				{
					case "0000":
					
						xfa.host.messageBox("The last 4 digits of SSN cannot be '0000'.", soExemption.FORM_NAME,1,0);
		    			objSSNPart.rawValue = "";
		    			break;
				}
			}
		}
	}		 	
}


/**
 * Checks for valid state abbreviation
 * and displays message and clears field if not valid 
 *  @param - oStateAbbr - state abbreviation field object
 *  @return - returns true if state abbreviation valid otherwise false
 */
function validateState(oStateAbbr)
{
	if ( oStateAbbr.rawValue != "" &amp;&amp; oStateAbbr.rawValue != null )
	{
		if ( !isValidState(oStateAbbr.rawValue) )
		{		
	   		xfa.host.messageBox("'" + oStateAbbr.rawValue + "' is not a valid state abbreviation.", soExemption.FORM_NAME,1,0);
	   		oStateAbbr.rawValue = "";
	   		xfa.host.setFocus(oStateAbbr);  		
			return false;
	   	}
   	}

	return true;
}


/**
 * Validates multi-part date and displays message and clears fields if not valid
 *  @param - oFldMonth - month field object
 *  @param - oFldDay - day field object
 *  @param - oFldYear - year field object 
 *  @return - returns true if date valid otherwise false
 */
function validateMultiPartDate(oFldMonth, oFldDay, oFldYear)
{
	if ( ( oFldMonth.rawValue != "" &amp;&amp; oFldMonth.rawValue != null ) &amp;&amp;
			( oFldDay.rawValue != "" &amp;&amp; oFldDay.rawValue != null ) &amp;&amp;
				( oFldYear.rawValue != "" &amp;&amp; oFldYear.rawValue != null ) )
	{
		if ( !isValidMultiPartDate(oFldMonth, oFldDay, oFldYear) )
		{		
   			xfa.host.messageBox("'" + oFldMonth.rawValue + "-" + oFldDay.rawValue + "-" + oFldYear.rawValue + "' is not a valid date.", soExemption.FORM_NAME,1,0);
   			oFldMonth.rawValue = "";
   			oFldDay.rawValue = "";
   			oFldYear.rawValue = "";
   			xfa.host.setFocus(oFldMonth);
			return false;
   		}
	}
	
	return true;
}


/**
 * Checks to see if birth date is less than 125 years from current and not in future
 *  @param - oFldMonth - month field object
 *  @param - oFldDay - day field object
 *  @param - oFldYear - year field object 
 *  @return - returns true if date valid otherwise false
 */
function checkMultiPartBirthDate(oFldMonth, oFldDay, oFldYear)
{
	if ( ( oFldMonth.rawValue != "" &amp;&amp; oFldMonth.rawValue != null ) &amp;&amp;
			( oFldDay.rawValue != "" &amp;&amp; oFldDay.rawValue != null ) &amp;&amp;
				( oFldYear.rawValue != "" &amp;&amp; oFldYear.rawValue != null ) )
	{
		var oCurrDt = new Date();
		oCurrDt.setHours(0,0,0,0);  //Clear time in current date object
		//app.alert("Curr Date: " + util.printd("mm/dd/yyyy", oCurrDt));
		
		var oPastDt = new Date();
		oPastDt.setFullYear(padDigits(oCurrDt.getFullYear()-125, 4), oCurrDt.getMonth(), oCurrDt.getDate());
		//app.alert("Past Date: " + util.printd("mm/dd/yyyy", oPastDt));
		
		var oNewDt = new Date (oFldYear.rawValue, oFldMonth.rawValue - 1, oFldDay.rawValue);
		//app.alert("New Date: " + util.printd("mm/dd/yyyy", oNewDt));

		if ( oNewDt &gt; oCurrDt )
		{
			xfa.host.messageBox("'" + oFldMonth.rawValue + "-" + oFldDay.rawValue + "-" + oFldYear.rawValue + "' birth date cannot be in the future.", soExemption.FORM_NAME,1,0);
			oFldMonth.rawValue = "";
			oFldDay.rawValue = "";
			oFldYear.rawValue = "";
			xfa.host.setFocus(oFldMonth);
			return false;
		}	
		else if ( parseInt(oFldYear.rawValue, 10) &lt; 100 || oNewDt.getFullYear() &lt; oPastDt.getFullYear() )  //Year sets to 1999 (and decrements) in date object if a year is entered as &lt;= 99 by user
		{
			xfa.host.messageBox("Birth date year of '" + padDigits(oFldYear.rawValue, 4) + "' cannnot be prior to " + oPastDt.getFullYear() + ".", soExemption.FORM_NAME,1,0);
			oFldMonth.rawValue = "";
			oFldDay.rawValue = "";
			oFldYear.rawValue = "";
			xfa.host.setFocus(oFldMonth);
			return false;
		}
	}
	
	return true;
}


/**
