         <event activity="enter" name="event__enter">
            <script contentType="application/x-javascript" runAt="both">
if ( soExemption.PERFORM_SECTIONAL_VALIDATION )
{
	soValidations.resetErrors();
	
	// Validate the first step
	if ( !soExemption.validateStep1(xfa.resolveNode("pgStep1.sfStep1[0]")) )
	{
		if ( soExemption.PERFORM_SECTIONAL_VALIDATION_ENFORCE_FILL ) 
		{
			// not correct
			xfa.host.messageBox("Please complete the form in Step 1 before proceeding to Step 2.\n\n" + soValidations.getErrorMessage(), soExemption.FORM_NAME, 1,0);
			soValidations.highlightFirstErrorField();
		}
	}
	else if ( !soExemption.validateStep2Person(xfa.resolveNode("pgStep2Person1.sfPerson1[0]"), 1) )
	{
		//Validate Person 1	
		if ( soExemption.PERFORM_SECTIONAL_VALIDATION_ENFORCE_FILL ) 
		{
			// soValidations.errorMessageBox();
			xfa.host.messageBox("Please complete the form for Person 1 before proceeding to Person 2.\n\n" + soValidations.getErrorMessage(), soExemption.FORM_NAME, 1,0);
			soValidations.highlightFirstErrorField();
		}
	}
	else
	{
		//Validate Step 2 - All Person 2s
		var intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
		
		if ( intStep2PeopleCount &gt; 0 )
		{	
			for( var i=0; i&lt;intStep2PeopleCount; i++ )
			{
				objNode = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s[" + i + "]");
				//app.alert("objNode: " + objNode.somExpression);
				if (!soExemption.validateStep2Person(objNode, i + 2))
				{
					if ( soExemption.PERFORM_SECTIONAL_VALIDATION_ENFORCE_FILL ) 
					{
						// soValidations.errorMessageBox();
						xfa.host.messageBox("Please complete the form for Person " + (i + 2) + " before proceeding to Step 3.\n\n" + soValidations.getErrorMessage(), soExemption.FORM_NAME, 1,0);
						soValidations.highlightFirstErrorField();
					}	
								
					break;
				}
			}
		}				
	}
}
</script>
