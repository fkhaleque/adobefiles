/*
 * As the value of the current field changes, test to see if current field value length is equal to
 * the current field's maxChar setting.  If it is, set focus to next the next tab-order field
 * specified in the current field.  This will effectively execute the exit event for the current
 * field which may contain required validations.
 *  @param - oCurrField - Current field object
 *  @param - sCurrFieldVal - Current field value
 */
function autoTab(oCurrField, sCurrFieldVal)
{
	try
	{
		//var currFieldMaxChars = oCurrField.value.text.maxChars;
		var currFieldMaxChars = oCurrField.ui.textEdit.comb.numberOfCells;
		//app.alert("Current Field Max: " + currFieldMaxChars);
		
		if ( currFieldMaxChars != 0 )
		{
			if ( sCurrFieldVal.length == currFieldMaxChars )
			{
				var sNextField = oCurrField.traversal.resolveNode("#traverse").ref;
				//app.alert("Next Tab Field: " + sNextField);
				
				if ( sNextField != null )
				{
					xfa.host.setFocus( sNextField );
				}
			}
		}
	}
	catch(e){}
}


/**
 * Pad numbers with '0' digits 
 *  @param - intNum - number to pad
 *  @param - intTotDigits - total digits to pad
 *  @return - padded number
 */
function padDigits(intNum, intTotDigits) 
{ 
	//console.println("'" + intNum + "'")

	var strNum = intNum.toString(); 
	var strPad = ""; 
	
	if ( intTotDigits &gt; strNum.length )
	{ 
		for ( j=0; j &lt; (intTotDigits-strNum.length); j++ )
		{ 
			strPad += "0"; 
		} 
	} 

	return strPad + strNum; 

}


