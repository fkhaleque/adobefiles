/**
 * Recalculates People # in header section of Step 2 sections
 * as people are removed or added via buttons
 *  @param - intIndexSkip - index count to skip (-1 does not skip any index)
 */
function recalcSectionTitle(intIndexSkip)
{
	var intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
	var intPersonCount = 2;
	var objNode = null;
	
	for( var i=0; i&lt;intStep2PeopleCount; i++ )
	{
		if ( i != intIndexSkip )
		{
			objNode = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s[" + i + "]");

			objNode.sfStepHeader.txtSectionTitle.rawValue = "STEP 2: Person " + intPersonCount;
			intPersonCount++;
		}
	}
}


/**
 * Sets Person 2 instances equal to the expected number of spouse and/or dependents
 *  @param - strDependents - Dependents from pgStep2Person1.sfPerson1.sfTaxes.listDependents
 */
function setPerson2Instances(strDependents)
{
	var intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
	var intPersonCount = getSpouseDependentCount(strDependents);	
	var objPage = xfa.resolveNode("pgStep2Person2");
	var objSubform = null;
	var intDiffCount = 0;
	var strMessage = "";

	//app.alert("intPersonCount: " + intPersonCount + " | intStep2PeopleCount " + intStep2PeopleCount);

	//Reset (hide) all Remove buttons in section 2 (-1 skips no indexes)
	toggleRemoveBttn(intStep2PeopleCount, -1, "hidden");

	if ( intPersonCount &gt;= intStep2PeopleCount )
	{	
		intDiffCount = intPersonCount - intStep2PeopleCount;

		//If pgStep2Person2 page is still hidden on initialize, then make visible		
		if ( objPage.presence == "hidden" )
		{
			//app.alert("pgStep2Person2: visible");
			objPage.presence = "visible";		
		}
		
		for( var i=1; i&lt;=intDiffCount; i++ )
		{
			//Use initial sfPerson2s instance or add one as necessary	

			//Add addtional instances if not equal to total people count
			if ( intPersonCount != intStep2PeopleCount )
			{
				//app.alert("sfPerson2s: adding instance");
				objSubform = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.addInstance(1);
			}
		}		

		//Refresh instance count
		intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
		
		//Recalculate section 2 titles (-1 skips no indexes)
		recalcSectionTitle(-1);
 		
 		//Display message
 		if ( intPersonCount == 1 )
 			strMessage = "You have 1 additional person identified in your application.\n\nBe sure to fill in a Step 2 Page for that person.";
 		else
 			strMessage = "You have " + intPersonCount + " additional people identified in your application.\n\nBe sure to fill in Step 2 Pages for each person.";
 		
 		if ( !soBarcoding.isOnServer() )
 		{
 			// dec 03 2015 wes 
 			// comment out; why message immediately after an action?
 			// also distracting on prefilled-onload; appears as "error"
 			// when none exists.
 			// xfa.host.messageBox(strMessage, soExemption.FORM_NAME,1,0);
 		}
 		
	}
	else //intPersonCount &lt; intStep2PeopleCount
	{
		var strFirstName = "";
		
		//Start from highest index and try to remove if no date (i.e., first name)
		for ( var i=intStep2PeopleCount-1; i&gt;=0; i-- )
		{	
			strFirstName = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s[" + i + "]").sfPerson.sfName.fldFirstName.rawValue;
			
			//Attempt to remove instances (keep at last instance if pgStep2Person2 is hidden)
			if ( strFirstName == "" || strFirstName == null )
			{
				if ( i == 0 &amp;&amp; intStep2PeopleCount == 1 )  //Hide pgStep2Person2 page only on last instance
				{
					objPage.presence = "hidden";
				}
				else
				{
					xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.removeInstance(i);
					intStep2PeopleCount--;
				}
				
				//If person count equals instance count, then fixed
				if ( intStep2PeopleCount == intPersonCount )
					break;
			}
		}

		//Refresh instance count
		//intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
		
		//Recalculate section 2 titles (-1 skips no indexes)
		recalcSectionTitle(-1);		
		
		//Let applicant decide what instance to remove if descrepancy not resolved		
		if ( objPage.presence != "hidden" &amp;&amp; intPersonCount &lt; intStep2PeopleCount )
		{
			//Show all Remove buttons in section 2 (-1 skips no indexes)
			toggleRemoveBttn(intStep2PeopleCount, -1, "visible");
										
	 		//Display message
	 		if ( intPersonCount == 0 )
	 			strMessage = "You should have no additional people identified in your application.";
	 		else if ( intPersonCount == 1 )
	 			strMessage = "You should have 1 additional person in your application.";
	 		else
	 			strMessage = "You should have " + intPersonCount + " additional people in your application."
	 		
			xfa.host.messageBox ("While adjusting Step 2 pages, it was detected that you started entering data.\n\n" +
									"Please manually remove Step 2 pages by clicking 'Remove this Person' button.\n\n" +
									strMessage, soExemption.FORM_NAME,1,0);									
		}
	}
}


/**
 * Gets accurate spouse and dependent count from Person 1 entries
 *  @param - strDependents - Dependents from pgStep2Person1.sfPerson1.sfTaxes.listDependents 
 *  @return - spouse and dependent count
 */
function getSpouseDependentCount(strDependents)
{
	var intPersonCount = 0;
	var objField = pgStep2Person1.sfPerson1.sfTaxes.rdoPlanToFile;
	
	if ( objField.rawValue != "N" &amp;&amp; objField.rawValue != "" &amp;&amp; objField.rawValue != null )
	{
		objField = pgStep2Person1.sfPerson1.sfTaxes.rdoFileJointly;
	
		if ( objField.rawValue == "Y" )
			intPersonCount++;
				
		if ( strDependents != "" &amp;&amp; strDependents != null )
			intPersonCount += parseInt(strDependents);
	}
	
	return intPersonCount;	
}


/**
 * Attempts to removes Person 2 section and checks/hides Remove button where necessary
 *  @param - intIndex - Index of subform to remove
 */
function removePerson2Page(intIndex)
{
	var objNodes = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s");
	var intStep2PeopleCount = objNodes.instanceManager.count;
	
	if ( intStep2PeopleCount &gt; 1 )
	{
		//Recalculate section titles prior to remove (provide index to skip)
		recalcSectionTitle(intIndex);
		
		//Hide Remove button if people count matches number of instances
		var strDependents = pgStep2Person1.sfPerson1.sfTaxes.listDependents.rawValue;
		var intPersonCount = getSpouseDependentCount(strDependents);
					
		if ( intPersonCount == ( intStep2PeopleCount - 1 ) )
			toggleRemoveBttn(intStep2PeopleCount, intIndex, "hidden");  //Skip removed instance's index
		
		//Remove selected instance
		objNodes.instanceManager.removeInstance(intIndex);
	}
	else
	{
		//If last instance, then hide pgStep2Person2 (keep at leaste one instance)
		var objPage = xfa.resolveNode("pgStep2Person2");
		objPage.presence = "hidden";
	}
	
	if ( intStep2PeopleCount &gt; 1 )
	{
		xfa.host.messageBox("This person has been removed from your application.\n\n" +
								"Any subsequent pages for other people will be moved up.", soExemption.FORM_NAME,1,0);
	}
	else
	{
		xfa.host.messageBox("This person has been removed from your application.", soExemption.FORM_NAME,1,0);
	}
	
}


/**
 * Sets the presence of the Remove button in Section 2
 *  @param - intStep2PeopleCount - sfPerson2s instance count
 *  @param - intIndexSkip - index count to skip (-1 does not skip any index)
 *  @param - strState - "visible" | "hidden"  
 */
function toggleRemoveBttn(intStep2PeopleCount, intIndexSkip, strState)
{
	var objNode = null;
	
	for ( var i=0; i&lt;intStep2PeopleCount; i++ )
	{
		if ( i != intIndexSkip )
		{	
			objNode = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s[" + i + "]");
			
			try
			{
				objNode.sfStepHeader.btnRemoveP2.presence = strState;	
			
				//If visible, set button so that it also does not show on print
				if ( strState == "visible" )
					objNode.sfStepHeader.btnRemoveP2.relevant = "-print";
				
			} catch (e) {}	
		}
	}
}


/******************************************************************
 *
 * Load / Save bound data from field parts - Hardship data
 *  
 ******************************************************************/

function loadHSBoundData(objParentDataNode, objHardshipsNode)
{
	var intNodeLen = objParentDataNode.nodes.length;
	var vItems = objHardshipsNode.resolveNodes("sfHardship[*]");
	
	//app.alert(intNodeLen);

	//Note: Child node indexes are zero-based, and there is always initally one node in the data DOM
	var intIndex = intNodeLen - 2;

	for ( var i=0; i&lt;=intIndex; i++ )
	{	
		var strChildNode = "hardship[" + i + "]";
		var objChildDataNode = objParentDataNode.resolveNode(strChildNode);	

		for( j=0; j&lt;vItems.length; j++ )
		{
			try
			{
				//If exemption specified, then process
				if ( objChildDataNode.type.value == j+1 )
				{				
					vItems.item(j).fldHS.rawValue = "1";
					try { vItems.item(j).fldHSTaxYear.rawValue = objChildDataNode.tax_year.value.substr(2,2); }  catch (e) { }
					try { vItems.item(j).fldHSDateStart.rawValue = objChildDataNode.start_date.value; }  catch (e) { }
					try { vItems.item(j).fldHSDateEnd.rawValue = objChildDataNode.end_date.value; }  catch (e) { }
					try { vItems.item(j).fldHSOngoing.rawValue = objChildDataNode.ongoing.value; }  catch (e) { }	
					
					break;
				}
			}
			catch (e) { }
		}
	}
}


function saveHSBoundData(objParentDataNode, objHardshipsNode)
{
	//Clear data DOM for parent node
	clearHSDataNodes(objParentDataNode);

	var vItems = objHardshipsNode.resolveNodes("sfHardship[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		//If exemption selected, then process
		if ( vItems.item(i).fldHS.rawValue &gt; 0 )
		{	
			try
			{	
				var objChildDataNode = getHSDataNodeObject(objParentDataNode);
				
				objChildDataNode.type.value = soValidations.padDigits(i+1, 2);
				
				//Save tax year
				if ( vItems.item(i).fldHSTaxYear.rawValue != null )
					objChildDataNode.tax_year.value = "20" + vItems.item(i).fldHSTaxYear.rawValue;	
				else
					objChildDataNode.tax_year.value = "";
				
				//Save date hardship started
				if ( vItems.item(i).fldHSDateStart.rawValue != null )		
					objChildDataNode.start_date.value = soValidations.getDateIsoFormat(vItems.item(i).fldHSDateStart);
				else
					objChildDataNode.start_date.value = "";				
				
				//Save date hardship ended
				if ( vItems.item(i).fldHSDateEnd.rawValue != null )
					objChildDataNode.end_date.value = soValidations.getDateIsoFormat(vItems.item(i).fldHSDateEnd);
				else
					objChildDataNode.end_date.value = "";						
				
				//Save ongoing flag		
				if ( vItems.item(i).fldHSOngoing.rawValue != null )
					objChildDataNode.ongoing.value = vItems.item(i).fldHSOngoing.rawValue;
				else
					objChildDataNode.ongoing.value = "";						
			}
			catch (e) {}
		}
	}
}


/**
 * Returns (next) hardship data object from data DOM in which to load hardhship info
 *  @param - objParentDataNode - Node to place data in DOM
 *  @return - new hardship data object from data DOM
 */
function getHSDataNodeObject(objParentDataNode)
{
	var intNodeLen = objParentDataNode.nodes.length;
	
	//app.alert(intNodeLen);

	//Note: Child node indexes are zero-based, and there is always initally one node in the data DOM
	var intIndex = intNodeLen - 2;
	var strChildNode = "hardship[" + intIndex + "]";
	var objChildDataNode = objParentDataNode.resolveNode(strChildNode);
	
	if ( objChildDataNode.type.value != "" &amp;&amp; objChildDataNode.type.value != null )
	{
		//Clone another data group node
		objParentDataNode.nodes.append( objParentDataNode.hardship.clone(1) );

		//Get data object for new clone
		intIndex++;
		strChildNode = "hardship[" + intIndex + "]";
		objChildDataNode = objParentDataNode.resolveNode(strChildNode);
	}

	return objChildDataNode;
		
}


/**
 * Clears hardship info in data node
 *  @param - objParentDataNode - Node to remove data in DOM
 */
function clearHSDataNodes(objParentDataNode)
{
	var intNodeLen = objParentDataNode.nodes.length;
	
	//app.alert(intNodeLen);

	//Note: Child node indexes are zero-based, and there is always initally one node in the data DOM
	var intIndex = intNodeLen - 2;

	for ( var i=intIndex; i&gt;=0; i-- )
	{
		var strChildNode = "hardship[" + i + "]";
		var objChildDataNode = objParentDataNode.resolveNode(strChildNode);
		
		if ( i == 0 )
		{
			//clear data values if only one child node - or create value nodes for those that are missing from AF
			try { objChildDataNode.type.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "type");
						objChildDataNode.nodes.append(oValueNode);	}
			
			try	{ objChildDataNode.tax_year.value = "";	}
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "tax_year");
						objChildDataNode.nodes.append(oValueNode);	}
			
			try { objChildDataNode.start_date.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "start_date");
						objChildDataNode.nodes.append(oValueNode);	}
																		
			try { objChildDataNode.end_date.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "end_date");
						objChildDataNode.nodes.append(oValueNode);	}
																		
			try { objChildDataNode.ongoing.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "ongoing");
						objChildDataNode.nodes.append(oValueNode);	}
		}
		else
			objParentDataNode.nodes.remove(objChildDataNode); 
	}
}


