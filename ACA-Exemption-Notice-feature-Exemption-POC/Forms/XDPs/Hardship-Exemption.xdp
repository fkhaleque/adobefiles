                  <event activity="prePrint" ref="$host" name="event__prePrint">
                     <script contentType="application/x-javascript" runAt="both">
// Concatenate Multi Fields
var strDOB = sfPerson.sfName.fldDOBMonth.formattedValue
			+ sfPerson.sfName.fldDOBDay.formattedValue
			+ sfPerson.sfName.fldDOBYear.formattedValue;

var strSSN = sfPerson.sfSSN.fldSSN1.formattedValue
			+ sfPerson.sfSSN.fldSSN2.formattedValue
			+ sfPerson.sfSSN.fldSSN3.formattedValue;

var strPageData = soBarcoding.encodePageMeta( soValidations.getPageAlpha(parseInt(Person2StartPg.value), this.parent.index) )
	+ soBarcoding.encodeString(sfPerson.sfName.fldFirstName.rawValue)
	+ soBarcoding.encodeString(sfPerson.sfName.fldMiddleName.rawValue)
	+ soBarcoding.encodeString(sfPerson.sfName.fldLastName.rawValue)
	+ soBarcoding.encodeString(sfPerson.sfName.fldSuffix.rawValue)
	+ soBarcoding.encodeString(sfPerson.sfName.fldRelationshipToPerson1.rawValue)
	+ soBarcoding.encodeString(strDOB)
	+ soBarcoding.encodeString(sfPerson.sfName.rdoSex.rawValue)
	+ soBarcoding.encodeString(strSSN)
	+ soBarcoding.encodeString(sfTaxes.rdoPlanToFile.rawValue)
	+ soBarcoding.encodeString(sfTaxes.rdoFileJointly.rawValue)
	+ soBarcoding.encodeString(sfTaxes.fldSpouse.rawValue)
	+ soBarcoding.encodeString(sfTaxes.rdoClaimdependents.rawValue)
	+ soBarcoding.encodeString(sfTaxes.flddependents.rawValue)
	+ soBarcoding.encodeString(sfTaxes.rdoOnP1Taxes.rawValue)
	+ soBarcoding.encodeString(sfTaxes.fldFiler.rawValue)
	+ soBarcoding.encodeString(sfTaxes.fldRelToFiler.rawValue)
