 *  @return - on server (true) | not on server (false) 
 */
function isOnServer()
{
	if ( xfa.host.name == "XFAPresentationAgent" )
		return true;
	else
		return false;
}


/**
 * Function to check that layout is accessible.
 * This will return false when rendered as an interactive PDF
