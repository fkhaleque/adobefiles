var intPage = xfa.layout.page(this) - xfa.resolveNodes("data.#pageSet.PageX[*]").length - 1;
		
var strPageData = soBarcoding.encodePageMeta( soExemption.getSeqAlphaNum(parseInt(Person2StartPg.value), intPage) )
	+ soBarcoding.encodeString(sfPerson.sfName.fldFirstName.rawValue)
