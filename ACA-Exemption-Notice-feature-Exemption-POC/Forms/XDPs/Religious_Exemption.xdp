/**
 * Checks for certain conditions with SSN that are not valid
 * and displays message and clears field if not valid 
 *  @param - field - field ID (SSN1 | SSN2 | SSN3)
 *  @param - objSSNPart - SSN part field object
 */
function validateSSNEntry(field, objSSNPart)
{
	if ( objSSNPart.rawValue != "" &amp;&amp; objSSNPart.rawValue != null )
	{
		if ( field == "SSN1" )
		{
			if ( objSSNPart.rawValue.charAt(0) == "9" )
			{
				xfa.host.messageBox("The first digit of SSN cannot cannot start with '9'.", soExemption.FORM_NAME,1,0);
		    	objSSNPart.rawValue = "";
			}
			else
			{	
				switch (objSSNPart.rawValue)
				{
					case "000":
					
						xfa.host.messageBox("The first 3 digits of SSN cannot be '000'.", soExemption.FORM_NAME,1,0);
		    			objSSNPart.rawValue = "";
		    			break;
					
					case "666":
					
						xfa.host.messageBox("The first 3 digits of SSN cannot be '666'.", soExemption.FORM_NAME,1,0);
		    			objSSNPart.rawValue = "";
		    			break;
				}
			}
		} 
		else if ( field == "SSN2" )
		{
			switch (objSSNPart.rawValue)
			{
				case "00":
				
					xfa.host.messageBox("The middle 2 digits of SSN cannot be '00'.", soExemption.FORM_NAME,1,0);
	    			objSSNPart.rawValue = "";
	    			break;
			}
		}  
		else if ( field == "SSN3" )
		{
			if ( objSSNPart.rawValue.substring(0,2) == "00" )
			{
				xfa.host.messageBox("The last 4 digits of SSN cannot begin with '00'.", soExemption.FORM_NAME,1,0);
		    	objSSNPart.rawValue = "";
			}
			else
			{	
				switch (objSSNPart.rawValue)
				{
					case "0000":
					
						xfa.host.messageBox("The last 4 digits of SSN cannot be '0000'.", soExemption.FORM_NAME,1,0);
		    			objSSNPart.rawValue = "";
		    			break;
				}
			}
		}
	}		 	
}


