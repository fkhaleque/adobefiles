function getRelPgBcodeStep3(objPage)
{
	//Get actual page count and PagePerson2s page count
	var intPageActual = xfa.layout.page(objPage);
	var intPagePerson2Count = xfa.resolveNodes("data.#pageSet.PagePerson2s[*]").length;
	
