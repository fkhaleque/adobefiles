/******************************************************************
 *
 * Doc Reminnder functions
 *  
 ******************************************************************/


function setDocReminders(arrReminders)
{
	/*
	 * arrReminders[0] = Person 1
	 * arrReminders[1] = Person 2
	 * arrReminders[2] = Person 3
	 * etc.
	 */
	
	if ( arrReminders.length &gt; 0 )
	{	
		for( var i=0; i&lt;arrReminders.length; i++ )
		{			
			if ( arrReminders[i] != "" )
			{		
				var objPage = null;
				var objSubform = null;
			
				//If pgSupportingDocs page is still hidden on initialize, then make visible
				//Get initial sfSupportingDocs instance or add one as necessary				
				objPage = xfa.resolveNode("Page1.pgSupportingDocs");
				
				if ( objPage.presence == "hidden" )
				{
					objPage.presence = "visible";
					objSubform = xfa.resolveNode("Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs[0]");				
				}	
				else
				{
					//app.alert("adding instance");	
					objSubform = Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.addInstance(1);									
				}		
						
				//app.alert("objSubform: " + objSubform.name + " | Index: " + objSubform.index);

				//Initialize person number and name of applicant
				if ( i == 0 )
				{
					objSubform.sfFormHeader.fldPersonX.rawValue = "Person 1 - Required documents for:";
					
					objSubform.sfFormHeader.fldPersonName.rawValue = assembleRemindPersonName(Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldFirstName, 
																							Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldMiddleName, 
																								Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldLastName, 
																									Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldSuffix);
				}
				else
				{
					objSubform.sfFormHeader.fldPersonX.rawValue = "Person " + (i + 1) + " - Required documents for:";
								
					objSubform.sfFormHeader.fldPersonName.rawValue = assembleRemindPersonName(Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldFirstName,
																							Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldMiddleName, 
																								Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldLastName, 
																									Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldSuffix);			
				}
					
				//Parse list of checkboxes and set visible or hidden
				var listReminderCheckboxNodes = objSubform.sfContent.sfReminders.resolveNodes("sfReminder[*]");
				//app.alert("listReminderCheckboxNodes: " + listReminderCheckboxNodes.length);
							
				for ( var j=0; j&lt;listReminderCheckboxNodes.length; j++ )
				{
					var strID = soValidations.padDigits(j + 1, 2);
					var intPos = arrReminders[i].indexOf(strID); 
					
					//Hide all checkbox subforms not found in string of IDs
					if ( intPos == -1 )
					{
						//console.println("Person " + (i + 1) + " | Hiding Reminder: " + (j + 1));					
						var objRemind = objSubform.sfContent.sfReminders.resolveNode("sfReminder[" + j + "]");		
						objRemind.presence = "hidden";
						objRemind.chkRemind.rawValue = "0";								
					}
				}
			}
		}
	}
}


function collectDocReminders()
{
	var objPage = xfa.resolveNode("Page1.pgStep2Person2");
	var arrReminders = new Array();	

	//Collect reminders Step 2 - First person
	arrReminders[0] = getReminderDocIDsForPerson(xfa.resolveNode("Page1.pgStep2Person1.sfPerson1[0]"));

	//If pgStep2Person2 is visible, then collect selected reminders
	if ( objPage.presence != "hidden" )
	{
		//Collect reminders Step 2 - All Person 2s
		var intStep2PeopleCount = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s").instanceManager.count;
			
		if ( intStep2PeopleCount &gt; 0 )
		{	
			var intArrCount = 1;
			
			for( var i=0; i&lt;intStep2PeopleCount; i++ )
			{
				objNode = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s[" + i + "]");
				arrReminders[intArrCount] = getReminderDocIDsForPerson(objNode);
				
				intArrCount++;
			}
		}
	}

	//soValidations.writeArray(arrReminders);
	
	return arrReminders;
}


function resetDocReminders()
{		
	var objPage = xfa.resolveNode("Page1.pgSupportingDocs");	
				
	//Hide pgSupportingDocs page
	objPage.presence = "hidden";		
	
	var intInstCount = Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.count;	
	
	//Remove sfSupportingDocs instances, but keep one (zero based)
	if ( intInstCount &gt; 1 )
	{
		for ( var i=intInstCount-1; i&gt;0; i-- )
		{
			Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.removeInstance(i);			
			//console.println("removing: " + i);		
		}
	}	

	//Get remaining sfSupportingDocs instance
	var objSubform = xfa.resolveNode("Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs[0]");

	//For last instance, clear description and name fields
	objSubform.sfFormHeader.fldPersonX.rawValue = "";
	objSubform.sfFormHeader.fldPersonName.rawValue = "";

	//For last instance, make visible and clear all checkboxes
	var listRemindCheckboxNodes = objSubform.sfContent.sfReminders.resolveNodes("sfReminder[*]");
	
	for ( var j=0; j&lt;listRemindCheckboxNodes.length; j++ )
	{
		//console.println("Resetting Reminder: " + (j + 1));
		var objRemind = objSubform.sfContent.sfReminders.resolveNode("sfReminder[" + j + "]");		
		objRemind.presence = "visible";
		objRemind.chkRemind.rawValue = "0";					
	}
}	


function getReminderDocIDsForPerson(objNode)
{
	var strDocReminderIDs = "";

	if ( objNode.sfMiscQuestions.sfQuestSect1.sfForm4029.rdoForm4029.rawValue == "Y" )
	{
		//Set all document reminders, and have customer indicate what is applicable on reminder page

		//IRS Form 4029 (ID = 01)	
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "01");
	}
	
	return strDocReminderIDs;
}


function appendReminderString(strDocReminderIDs, strNewReminderID)
{
	if ( strDocReminderIDs == "" )
		strDocReminderIDs = strNewReminderID;	
	else
		strDocReminderIDs += "," + strNewReminderID;
		
	return strDocReminderIDs;
}


function assembleRemindPersonName(objFirstNm, objMiddleNm, objLastNm, objSuffixNm)
{
	var strFullName = "";
	
	if ( objFirstNm.rawValue != "" &amp;&amp; objFirstNm.rawValue != null )
		strFullName += objFirstNm.rawValue;
	
	if ( objMiddleNm.rawValue != "" &amp;&amp; objMiddleNm.rawValue != null )
		strFullName += " " + objMiddleNm.rawValue;
		
	if ( objLastNm.rawValue != "" &amp;&amp; objLastNm.rawValue != null )
		strFullName += " " + objLastNm.rawValue;		

	if ( objSuffixNm.rawValue != "" &amp;&amp; objSuffixNm.rawValue != null )
		strFullName += ", " + objSuffixNm.rawValue;	
	
	return soValidations.trimString(strFullName);
}


