 *   @param - intPageNum - page number
 */
function encodePageMeta(intPageNum)
{
	return soExemption.BC_FORM_NAME + ";" + intPageNum + ";";
}


/**
 * Encodes string data for QR barcode
 *  @param - strData - string data generated for barcode
 *  @return - encoded string 
 */
function encodeString(strData)
{
	//console.println("IN strData: " + strData);
	
	if ( strData != null )
	{
		strData = strData.toString();
		
		//if not empty field, add separator to data
		if ( strData != "" )
		{
			//remove any delimeters within string before returning	
			strData.replace(/;/g,"");
			strData += ";";
	
			//console.println("OUT strData: " + strData);
	
			return strData;	
		}
	}

	return ";";
}


/**
 * Encodes date in format mmddyyyy for QR barcode
 *  @param - strDate - date string in form yyyy-mm-dd
 *  @return - new string date in form mmddyyyy
 */
function encodeDate(strDate)
{
	//console.println("attempting to encode " + strDate);
	
	//Load moment.js
	var moment = momentJS.moment();
				
	if ( moment(strDate, "YYYY-MM-DD", true).isValid() )
	{
		//If parsed, print back as mmddyyyy string
		var strNewDate = moment(strDate, "YYYY-MM-DD", true).format("MMDDYYYY") + ";";
		
		//console.println("encoded date: " + strNewDate);
		
		return strNewDate;
	}	
	
	return ";";
}


/**
