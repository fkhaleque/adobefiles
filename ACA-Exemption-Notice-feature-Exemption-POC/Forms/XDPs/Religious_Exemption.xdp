/**
 * Checks required field for "" or null value
 *  @param - field - Field object to check
 *  @param - errmsg - Error message to display
 *  @return - Returns true if blank/null; otherwise, false
 */
function checkReqFieldIsBlank(field, errmsg)
{
	if ( field.rawValue == "" || field.rawValue == null )
	{
		setFieldErrorState(field, true);
	
		// Are we showing errors, and do we have a message to show?
		if ( SHOW_ALERTS &amp;&amp; errmsg != null &amp;&amp; errmsg.length &gt; 0 )
		{
			xfa.host.messageBox(errmsg,soExemption.FORM_NAME, 1, 0);
		}

		return true;
	}

	setFieldErrorState(field, false);
	
	return false;
}


/**
 * Gets valid integer number data from number field
 *  @param - oNumField - Field number object
 *  @return - valid integer number
 */
function getNumInt(oNumField)
{
    var zeroNum = 0;
    var txtNum = oNumField.rawValue;
    var intNum = parseInt(txtNum);
    
    if ( ( txtNum == "" ) || ( txtNum == null ) || isNaN(intNum) )
        return zeroNum;
    else 
        return intNum;
}


/**
 * Gets valid floating number data from number field
 *  @param - oNumField - Field number object
 *  @param - intDecPos - number of decimals in returned number 
 *  @return - valid integer number
 */
function getNumFloat(oNumField, intDecPos)
{
    var zeroNum = 0;
    var txtNum = oNumField.rawValue;
    var intNum = parseFloat(txtNum);
    
    if ( ( txtNum == "" ) || ( txtNum == null ) || isNaN(intNum) )
    	return zeroNum.toFixed(intDecPos);
    else 
        return intNum.toFixed(intDecPos);
}


