</script>
                           </event>
                           <event activity="ready" ref="$layout" name="event__layout_ready">
                              <script contentType="application/x-javascript">
//Populate barcode when rendered on server
if ( soBarcoding.isOnServer() &amp;&amp; soBarcoding.isLayedOut() )
{
	var objSubform = this.parent.parent;
	var vItems = objSubform.sfReminders.resolveNodes("sfReminder[*]");
	
	//Encode barcode
	var strPageData = soBarcoding.encodePageMeta( soExemption.getSeqAlphaNum(parseInt(SuppDocsStartPg.value), this.parent.parent.parent.index) )
					+ soBarcoding.encodeString(sfFormHeader.fldPersonName.rawValue);
	
	var objRemindField = null;
	var intSelected = 0;
	
	for ( var i=0; i&lt;vItems.length; i++ )
	{
		if ( vItems.item(i).presence == "visible" )
		{
			strPageData += soBarcoding.encodeString(i + 1) 
						+ soBarcoding.encodeString(vItems.item(i).chkRemind.rawValue);
		
			intSelected++;
		}
	}
	
	//Per requirements, a max of 5 reminders is to be assumed
	for ( i=intSelected; i&lt;5; i++ )
	{
		//Pad-out data with empty separators
		strPageData += ";;";
	}
	
	this.rawValue = strPageData;
	
	//Show barcode
	this.presence = "visible";	
}
</script>
