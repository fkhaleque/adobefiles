function setDocReminders(arrReminders)
{
	/*
	 * arrReminders[0] = Person 1
	 * arrReminders[1] = Person 2
	 * arrReminders[2] = Person 3
	 * etc.
	 */
	
	if ( arrReminders.length &gt; 0 )
	{	
		for( var i=0; i&lt;arrReminders.length; i++ )
		{			
			if ( arrReminders[i] != "" )
			{		
				var objPage = null;
				var objSubform = null;
			
				//If pgSupportingDocs page is still hidden on initialize, then make visible
				//Get initial sfSupportingDocs instance or add one as necessary				
				objPage = xfa.resolveNode("Page1.pgSupportingDocs");
				
				if ( objPage.presence == "hidden" )
				{
					objPage.presence = "visible";
					objSubform = xfa.resolveNode("Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs[0]");				
				}	
				else
				{
					//app.alert("adding instance");	
					objSubform = Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.addInstance(1);									
				}		
						
				//app.alert("objSubform: " + objSubform.name + " | Index: " + objSubform.index);

				//Initialize person number and name of applicant
				if ( i == 0 )
				{
					objSubform.sfFormHeader.fldPersonX.rawValue = "Person 1 - Required documents for:";
					
					objSubform.sfFormHeader.fldPersonName.rawValue = assembleRemindPersonName(Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldFirstName, 
																							Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldMiddleName, 
																								Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldLastName, 
																									Page1.pgStep2Person1.sfPerson1.sfStepHeader.sfPerson.sfName.fldSuffix);
				}
				else
				{
					objSubform.sfFormHeader.fldPersonX.rawValue = "Person " + (i + 1) + " - Required documents for:";
								
					objSubform.sfFormHeader.fldPersonName.rawValue = assembleRemindPersonName(Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldFirstName,
																							Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldMiddleName, 
																								Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldLastName, 
																									Page1.pgStep2Person2.resolveNode("sfPerson2s[" + (i - 1) + "]").sfStepHeader.sfPerson.sfName.fldSuffix);			
				}
					
				//Parse list of checkboxes and set visible or hidden
				var listReminderCheckboxNodes = objSubform.sfContent.sfReminders.resolveNodes("sfReminder[*]");
				//app.alert("listReminderCheckboxNodes: " + listReminderCheckboxNodes.length);
							
				for ( var j=0; j&lt;listReminderCheckboxNodes.length; j++ )
				{
					var strID = soValidations.padDigits(j + 1, 2);
					var intPos = arrReminders[i].indexOf(strID); 
					
					//Hide all checkbox subforms not found in string of IDs
					if ( intPos == -1 )
					{
						//console.println("Person " + (i + 1) + " | Hiding Reminder: " + (j + 1));					
						var objRemind = objSubform.sfContent.sfReminders.resolveNode("sfReminder[" + j + "]");		
						objRemind.presence = "hidden";
						objRemind.chkRemind.rawValue = "0";								
					}
				}
			}
		}
	}
}


function collectDocReminders()
{
	var objPage = xfa.resolveNode("Page1.pgStep2Person2");
