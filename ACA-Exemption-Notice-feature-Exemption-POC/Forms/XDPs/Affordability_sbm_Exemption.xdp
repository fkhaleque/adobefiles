/**
 * Parse all root objects defined in string and set fields to desired access setting which
 * is either protected (locked) or optional (open)
 *	@param - strAccessSetting - "lock" (protected) or "open" (optional)
 *	@param - strSubformList - set of all subforms (separated by ';') to parse and
 *           lock/open fields as specified by setting param
 *	@param - strExcludeToken - text added at the end of a field/button name so that
 *           it is identified and excluded from being locked
 *           (or opened). Ex. For "Button1_nolock", "_nolock" could
 *           be the exclude token if desired.
 */ 
function setNodeAccessExclude(strAccessSetting, strSubformList, strExcludeToken)
{	

	// Get nodes and parse
	var arrListNodes = strSubformList.split(";");
	var intNumListNodes = arrListNodes.length;

	for ( var i=0; i&lt;intNumListNodes; i++ )
	{
		var strListNode = arrListNodes[i];
		var objNode = xfa.resolveNode(strListNode);
		setFieldAccessExclude(strAccessSetting, objNode, strExcludeToken);
	}
}


/**
 * Called by setNodeAccessExclude and recursively called by itself, starting with
 * the root obj, set all contained fields to protected (locked) or optional (open)
 *	@param - strAccessSetting - "lock" (protected) or "open" (open)
 *	@param - objNode - Node to parse and lock/open fields as specified by setting param
 *	@param - strExcludeToken - exclusion text token at the end of field/button name
 */ 
function setFieldAccessExclude(strAccessSetting, objNode, strExcludeToken)
{	

	//Get all the child nodes of the parent element excluding a node if necessary
	var allChildElements = objNode.nodes;
	var intNumElements = allChildElements.length;
	
	//parse objects and set to protected
	for( var j=0; j&lt;intNumElements; j++ )
	{	
		var objItem = allChildElements.item(j);

		if( objItem.className == "subform" ) { setFieldAccessExclude(strAccessSetting, objItem, strExcludeToken); continue; }
		if( objItem.className == "exclGroup" ) { setFieldAccessExclude(strAccessSetting, objItem, strExcludeToken); continue; }
		if( objItem.className != "field"  ) continue;

		var intNameLen = objItem.name.length;
		var intTokenLen = strExcludeToken.length;
		
		//Exclude fields/buttons whose name does not include the exclude token
		if ( objItem.name.substr(intNameLen - intTokenLen, intTokenLen) != strExcludeToken )
		{
			// make objects protected while excluding necessary buttons
			if ( strAccessSetting == "lock" )
				objItem.access = "protected";   // make object protected
			else
				objItem.access = "open";  // make object optional
		}
	}
}


/**
 * Clear and write array contents to JS console (DEBUG)
 *	@param - arrStrings - array of strings
 */
function writeArray(arrStrings)
{
	console.clear();
	for( var j=0; j&lt;arrStrings.length; j++ )
		console.println("|" + arrStrings[j] + "|");
}
</script>
