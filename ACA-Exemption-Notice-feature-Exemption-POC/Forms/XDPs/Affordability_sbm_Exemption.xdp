               <event activity="exit" name="event__exit">
                  <script contentType="application/x-javascript">
// Validate section 
if ( soExemption.PERFORM_SECTIONAL_VALIDATION )
{
	//Validate Step 2 if pgStep2Person2 is visible - All Person 2s
	var intStep2PeopleCount = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s").instanceManager.count;
	var objPage = xfa.resolveNode("Page1.pgStep2Person2");	

	if ( intStep2PeopleCount &gt; 0 &amp;&amp; objPage.presence != "hidden" )
	{	
		var flgShowReminderMsg = true;
		
		for( var i=0; i&lt;intStep2PeopleCount; i++ )
		{
			var strSectionID = "STEP2_P" + (i + 2);
			
			//Validate section if current section ID is this section (or not set)
			if ( soValidations.getEvalSection() == strSectionID || soValidations.getEvalSection() == "" )
			{
				soValidations.resetErrors();
				soValidations.setEvalSection(strSectionID);
			
				objNode = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s[" + i + "]");
				//app.alert("objNode: " + objNode.somExpression);
				
				if ( !soExemption.validateStep2Person(objNode, i + 2) )
				{
					if (soExemption.PERFORM_SECTIONAL_VALIDATION_ENFORCE_FILL) 
					{
						// soValidations.errorMessageBox();
						xfa.host.messageBox("Please complete the form for Person " + (i + 2) + " before proceeding.\n\n" + soValidations.getErrorMessage(), soExemption.FORM_NAME, 1,0);
						soValidations.highlightFirstErrorField();
						break;
					}
				}
				else
				{
					soValidations.setEvalSection("");   //Reset section validation
					
					//Reset doc reminders
					soExemption.resetDocReminders();
					var intIndex = this.index + 1;
					
					//Generate doc reminders
					var arrReminders = new Array();	
					arrReminders = soExemption.collectDocReminders();
					soExemption.setDocReminders(arrReminders);
								
					if ( arrReminders.length &gt; 0 &amp;&amp; intIndex != null &amp;&amp; flgShowReminderMsg )
					{
						if ( arrReminders[intIndex] != "" )
						{
							xfa.host.messageBox("There is a document reminder cover page that was generated for Person " + (intIndex + 1) +
													" at the end of this application.\n\n" +
														"Please fill-in the reminder cover page prior to print.", soExemption.FORM_NAME,1,0);
						
							flgShowReminderMsg = false;  //Show document reminder message only once
						}
					}				
				}
			}
		}
	}
}

//console.println("Page1.pgStep2Person2::exit: " + soValidations.getEvalSection());
</script>
               </event>
