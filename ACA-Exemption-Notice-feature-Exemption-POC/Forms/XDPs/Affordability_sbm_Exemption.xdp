	if ( intInstCount &gt; 1 )
	{
		for ( var i=intInstCount-1; i&gt;0; i-- )
		{
			Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.removeInstance(i);			
			//console.println("removing: " + i);		
		}
	}	

	//Get remaining sfSupportingDocs instance
	var objSubform = xfa.resolveNode("Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs[0]");

	//For last instance, clear description and name fields
	objSubform.sfFormHeader.fldPersonX.rawValue = "";
	objSubform.sfFormHeader.fldPersonName.rawValue = "";

	//For last instance, make visible and clear all checkboxes
	var listRemindCheckboxNodes = objSubform.sfContent.sfReminders.resolveNodes("sfReminder[*]");
	
	for ( var j=0; j&lt;listRemindCheckboxNodes.length; j++ )
	{
		//console.println("Resetting Reminder: " + (j + 1));
		var objRemind = objSubform.sfContent.sfReminders.resolveNode("sfReminder[" + j + "]");		
		objRemind.presence = "visible";
		objRemind.chkRemind.rawValue = "0";					
	}
}	


function getReminderDocIDsForPerson(objNode)
{
	var strDocReminderIDs = "";

	if ( soValidations.getNumFloat(objNode.sfMiscQuestions.sfQuestSect1.sfIncomeYearly.fldTotIncomeThisYear, 2).toString() != "0.00" &amp;&amp;
			objNode.sfMiscQuestions.sfQuestSect1.sfIncomeYearly.fldTotIncomeThisYear.formattedValue.replace(/^\s+|\s+$/g,'') != "$0.00" )
	{
		//Set all document reminders, and have customer indicate what is applicable on reminder page

		//Income Type: Job (ID = 01)	
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "01");
		
		//Income Type: All Income Types (ID = 02)		
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "02");			

		//Income Type: Net Self-Employed (ID = 03)
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "03");

		//Other Income: Unemployment (ID = 04)
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "04");  
		
		//Other Income: Retirement (ID = 05)
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "05");  
		
		//Other Income: Pensions (ID = 06)
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "06");  	
		
		//Other Income: Rental/Royalties (ID = 07)	
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "07");  	
		
		//Other Income: Alimony (ID = 08)
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "08");  
		
		//Other Income: Farming/Fishing (ID = 09)	
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "09");  			
		
		//Other Income: Social Security (ID = 10)
		strDocReminderIDs = appendReminderString(strDocReminderIDs, "10");  
	}
	
	return strDocReminderIDs;
}


function appendReminderString(strDocReminderIDs, strNewReminderID)
{
	if ( strDocReminderIDs == "" )
		strDocReminderIDs = strNewReminderID;	
	else
		strDocReminderIDs += "," + strNewReminderID;
		
	return strDocReminderIDs;
}


function assembleRemindPersonName(objFirstNm, objMiddleNm, objLastNm, objSuffixNm)
{
	var strFullName = "";
	
	if ( objFirstNm.rawValue != "" &amp;&amp; objFirstNm.rawValue != null )
		strFullName += objFirstNm.rawValue;
	
	if ( objMiddleNm.rawValue != "" &amp;&amp; objMiddleNm.rawValue != null )
		strFullName += " " + objMiddleNm.rawValue;
		
	if ( objLastNm.rawValue != "" &amp;&amp; objLastNm.rawValue != null )
		strFullName += " " + objLastNm.rawValue;		

	if ( objSuffixNm.rawValue != "" &amp;&amp; objSuffixNm.rawValue != null )
		strFullName += ", " + objSuffixNm.rawValue;	
	
	return soValidations.trimString(strFullName);
}


