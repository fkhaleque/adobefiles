		{	
			soValidations.addError(objNode.sfEmployeeInformation.sfEmpInfo.fldSSN1, "APPENDIX A - PAGE " + strPage, "- Social Security Number is not valid");
			soValidations.setFieldErrorState(objNode.sfEmployeeInformation.sfEmpInfo.fldSSN1, true);
			soValidations.setFieldErrorState(objNode.sfEmployeeInformation.sfEmpInfo.fldSSN2, true);
			soValidations.setFieldErrorState(objNode.sfEmployeeInformation.sfEmpInfo.fldSSN3, true);
		}	

		if ( soValidations.checkReqFieldIsBlank(objNode.sfEmployerInformation.sfEmployerInfo.sfEmployerName.fldEmployerName, "") )
	    {
	        soValidations.addError(objNode.sfEmployeeInformation.sfEmpInfo.fldLastName, "APPENDIX A - PAGE " + strPage, "- You must select enter an Employer Name");
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfEmployerName.fldEmployerName, true);	        
	    }	

		//Check for validity EIN
		var validEIN = soValidations.isValidEIN(objNode.sfEmployerInformation.sfEmployerInfo.sfEIN.sfNum.fldEIN1.editValue + objNode.sfEmployerInformation.sfEmployerInfo.sfEIN.sfNum.fldEIN2.editValue );

		if ( !validEIN )
		{	
			soValidations.addError(objNode.sfEmployerInformation.sfEmployerInfo.sfEIN.sfNum.fldEIN1, "APPENDIX A - PAGE " + strPage, "- Employer ID Number (EIN) is not valid");
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfEIN.sfNum.fldEIN1, true);
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfEIN.sfNum.fldEIN2, true);
		}	

		var addressEmpty = soValidations.areTheseFieldsEmpty([objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerAddress,
														objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerCity, objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerState,
														objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerZipCode]);


		var addressValid = soValidations.isEmployerAddressValid(objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerAddress.rawValue,
														objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerCity.rawValue,
														objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerState.rawValue,
														objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerZipCode.rawValue,
														false);

		//Check if address is invalid
		if ( !addressValid  )
		{
			//Re-run the validations, this time, highlighting the broken fields
			soValidations.isValidEmployerAddress(objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerAddress, 
										objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerCity,
										objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerState,
										objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerZipCode,
										true);
										
			soValidations.addError(objNode.sfEmployerInformation.sfEmployerInfo.sfAddress.fldEmployerAddress, "APPENDIX A - PAGE " + strPage, "- Must specify a valid employer address");
		
		}	
	
		//Check for valid employer phone number
		var emptyPhone = soValidations.areTheseFieldsEmpty([objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldPhoneArea,
															objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldPhonePrefix,
															objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldPhoneSuffix]);
		
		if ( emptyPhone )
		{
			//No phone number was filled
			soValidations.addError(objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldPhoneArea, "APPENDIX A - PAGE " + strPage, "- Must specify a valid employer phone number");
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldPhoneArea, true);
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldPhonePrefix, true);	
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldPhoneSuffix, true);			
		}	
	
		if ( soValidations.checkReqFieldIsBlank(objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldEmployerContact, "") )
	    {
	        soValidations.addError(objNode.sfEmployeeInformation.sfEmpInfo.fldLastName, "APPENDIX A - PAGE " + strPage, "- Employer contact is empty");
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfContact.fldEmployerContact, true);
	    }	
	
		//Check for valid employer conatct phone number
		var emptyContactPhone = soValidations.areTheseFieldsEmpty([objNode.sfEmployerInformation.sfEmployerInfo.sfPhone.fldEmployerContactPhoneArea,
																	objNode.sfEmployerInformation.sfEmployerInfo.sfPhone.fldEmployerContactPhonePrefix,
																	objNode.sfEmployerInformation.sfEmployerInfo.sfPhone.fldEmployerContactPhoneSuffix]);
		
		if ( emptyContactPhone )
		{
			//No phone number was filled
			soValidations.addError(objNode.sfEmployerInformation.sfEmployerInfo.sfPhone.fldEmployerContactPhoneArea, "APPENDIX A - PAGE " + strPage, "- Must specify a valid employer contact phone number");
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfPhone.fldEmployerContactPhoneArea, true);
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfPhone.fldEmployerContactPhonePrefix, true);	
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfPhone.fldEmployerContactPhoneSuffix, true);			
		}
	

		//Test if contact email is valid and of correct syntax
		var validEmail = soValidations.isValidEmail(objNode.sfEmployerInformation.sfEmployerInfo.sfEmail.fldEmployerContactEmail.rawValue);
			
		if ( !validEmail )
		{
			soValidations.addError(objNode.sfEmployerInformation.sfEmployerInfo.sfEmail.fldEmployerContactEmail, "APPENDIX A - PAGE " + strPage, "- Employer contact email address is invalid");
			soValidations.setFieldErrorState(objNode.sfEmployerInformation.sfEmployerInfo.sfEmail.fldEmployerContactEmail, true);		
		}

		
		//Check question 13
		if ( soValidations.checkReqFieldIsBlank(objNode.sfEligibleEmpCoverage.rdoEmployeeEligible, "") )
	    {
	        soValidations.addError(objNode.sfEligibleEmpCoverage.rdoEmployeeEligible, "APPENDIX A - PAGE " + strPage, "- Must specifiy if you are eligible for coverage");
			soValidations.setFieldErrorState(objNode.sfEligibleEmpCoverage.rdoEmployeeEligible, true);	        
	    }			
		else
		{
			if ( objNode.sfEligibleEmpCoverage.rdoEmployeeEligible.rawValue == "Y" )
			{
	
				//Check for validity of probation period date
				var validDate = soValidations.isValidDate(objNode.sfEligibleEmpCoverage.fldEligibleStartYear.rawValue,
															objNode.sfEligibleEmpCoverage.fldEligibleStartMonth.rawValue,
															objNode.sfEligibleEmpCoverage.fldEligibleStartDay.rawValue);
			
