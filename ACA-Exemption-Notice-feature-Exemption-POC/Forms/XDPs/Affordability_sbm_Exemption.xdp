	    {
	        soValidations.addError(objNode.sfEmployeeInformation.sfEmpInfo.fldLastName, "APPENDIX A - PAGE " + strPage, "- Last name is empty");
			soValidations.setFieldErrorState(objNode.sfEmployeeInformation.sfEmpInfo.fldLastName, true);	        
	    }	
	    	
		//Check for validity SSN
