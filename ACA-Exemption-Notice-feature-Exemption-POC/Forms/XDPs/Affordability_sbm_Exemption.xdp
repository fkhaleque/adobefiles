/**
 * Gets total page count for page numbers
 *  @return - total page count
 */
function getTotalPageCount()
{
	//Get page count for static pages
	var intPageTot = xfa.resolveNodes("data.#pageSet.PageX[*]").length +
						xfa.resolveNodes("data.#pageSet.PageY[*]").length + 
							xfa.resolveNodes("data.#pageSet.PageZ[*]").length;
						
	//Increment if section PagePerson2s &gt; 0
	if ( xfa.resolveNodes("data.#pageSet.PagePerson2s[*]").length &gt; 0 )
		intPageTot++;
		
	//Increment if section Appendix A &gt; 0	
	if ( xfa.resolveNodes("data.#pageSet.AppendixAs[*]").length &gt; 0 )
		intPageTot++;
	
	//Increment if section PageSuppDocs &gt; 0	
	if ( xfa.resolveNodes("data.#pageSet.PageSuppDocs[*]").length &gt; 0 )
		intPageTot++;
	
	return intPageTot;
}


/**
 * Gets starting page relative to all prior pages for barcode step 3 (Low Cost Plan)
 *  @param - objPage - page in which barcode resides 
 *  @return - start page for barcode
 */
function getRelPgBcodeStep3(objPage)
{
	//Get actual page count and PagePerson2s page count
	intPageActual = xfa.layout.page(objPage);
	intPagePerson2Count = xfa.resolveNodes("data.#pageSet.PagePerson2s[*]").length;
	
	//Continue page number relative to current page - previous PagePerson2s page count
	var intPage = intPageActual - intPagePerson2Count;
	
	//Increment if section PagePerson2s exists
	if ( intPagePerson2Count &gt; 0 )
		intPage++;
	
	return intPage;
}


