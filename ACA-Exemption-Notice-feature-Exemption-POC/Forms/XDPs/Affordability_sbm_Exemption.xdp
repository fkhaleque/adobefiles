					soValidations.setFieldErrorState(objNode.sfEligibleEmpCoverage.fldEligibleStartYear, true);
				}
		
		
				//Check for indication of employer plan meeting minimum value standard
				if ( soValidations.checkReqFieldIsBlank(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect1.rdoEmloyerOfferedHP, "") )
				{
					soValidations.addError(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect1.rdoEmloyerOfferedHP, "APPENDIX A - PAGE " + strPage, "- Must indicate if employer health plan meets minimum standard");
					soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect1.rdoEmloyerOfferedHP, true);
				}
		
				
				//Check question 15
				if ( objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect2.fldCostofPremiums.rawValue != "" &amp;&amp;
						objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect2.fldCostofPremiums.rawValue != null )
			    {
			    	if ( soValidations.checkReqFieldIsBlank(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect2.radioFrequency, "") )
			    	{
			        	soValidations.addError(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect2.radioFrequency, "APPENDIX A - PAGE " + strPage, "- Must specify premium frequency");
						soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect2.radioFrequency, true);
					}
			    }	
		
		
				//Check question 16
				if ( objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect3.fldCostofPRemiums2.rawValue != "" &amp;&amp;
						objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect3.fldCostofPRemiums2.rawValue != null )
			    {
			    	if ( soValidations.checkReqFieldIsBlank(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect3.radioFrequency, "") )
			    	{
			        	soValidations.addError(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect3.radioFrequency, "APPENDIX A - PAGE " + strPage, "- Must specify premium frequency");
						soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect3.radioFrequency, true);
					}
			    }
		
		
				//Check question 17
				if ( soValidations.checkReqFieldIsBlank(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.rdoCoverageChanges, "") )
				{
					soValidations.addError(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.rdoCoverageChanges, "APPENDIX A - PAGE " + strPage, "- Must indicate if employer will make change to new plan");
					soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.rdoCoverageChanges, true);
				}
				else
				{
					if ( objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.rdoCoverageChanges.rawValue == "Y" )
					{
		
						if ( objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.sfLCPlanSect4a.fldCostofPremiums3.rawValue != "" &amp;&amp;
								objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.sfLCPlanSect4a.fldCostofPremiums3.rawValue != null )
					    {
					    	if ( soValidations.checkReqFieldIsBlank(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.sfLCPlanSect4a.radioFrequency, "") )
					    	{
					        	soValidations.addError(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.sfLCPlanSect4a.radioFrequency, "APPENDIX A - PAGE " + strPage, "- Must specify premium frequency");
								soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.sfLCPlanSect4a.radioFrequency, true);
							}
					    }		    
					    
						//Check for validity of change date
						var validDate = soValidations.isValidDate(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.fldChangeYear.rawValue,
																	objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.fldChangeMonth.rawValue,
																	objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.fldChangeDay.rawValue);
					
						if ( !validDate )
						{	
							soValidations.addError(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.fldChangeMonth, "APPENDIX A - PAGE " + strPage, "- Change date is not valid");
							soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.fldChangeMonth, true);
							soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.fldChangeDay, true);	
							soValidations.setFieldErrorState(objNode.sfLowestCostPlan.sfLCPlanInfo.sfLCPlanSect4.fldChangeYear, true);
						}
						
					}
				}
			}
		}
	}
	
	return soValidations.isValid();	
}


