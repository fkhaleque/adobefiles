	//Collect reminders Step 2 - First person
	arrReminders[0] = getReminderDocIDsForPerson(xfa.resolveNode("Page1.pgStep2Person1.sfPerson1[0]"));

	//If pgStep2Person2 is visible, then collect selected reminders
	if ( objPage.presence != "hidden" )
	{
		//Collect reminders Step 2 - All Person 2s
		var intStep2PeopleCount = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s").instanceManager.count;
			
