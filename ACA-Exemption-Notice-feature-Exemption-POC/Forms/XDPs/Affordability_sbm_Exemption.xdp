{		
	var objPage = xfa.resolveNode("Page1.pgSupportingDocs");	
				
	//Hide pgSupportingDocs page
	objPage.presence = "hidden";		
	
	var intInstCount = Page1.pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.count;	
	
