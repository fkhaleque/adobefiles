/**
 * Sets the presence of the Remove button in Appendix A
 *  @param - objAppendixA - Appendix A section object
 *  @param - intIndexSkip - index count to skip (-1 does not skip any index)
 */
function toggleRemoveAppendixABttn(objAppendix, intIndexSkip)
{
	var intInstCount = objAppendix.sfAppendixAs.instanceManager.count;
	var objNode = null;
	
	if ( intInstCount == 1 )
	{
		//Hide Remove button on single instance
		objAppendix.resolveNode("sfAppendixAs[0]").sfFormHeader.bttnRemoveAppendix.presence = "hidden";
	}
	else if ( intInstCount == 2 &amp;&amp; intIndexSkip != -1 )
	{
		//Where only one instance would be remaining after removal,
		//hide Remove button on remianing instance prior to removing the other
		if ( intIndexSkip == 0 )
			objNode = objAppendix.resolveNode("sfAppendixAs[1]");
		else
			objNode = objAppendix.resolveNode("sfAppendixAs[0]");
			
		objNode.sfFormHeader.bttnRemoveAppendix.presence = "hidden";
	}
	else
	{
		//Make all Remove buttons visible except the one to skip
		for ( var i=0; i&lt;intInstCount; i++ )
		{
			if ( i != intIndexSkip )
			{	
				objNode = objAppendix.resolveNode("sfAppendixAs[" + i + "]");
	
				objNode.sfFormHeader.bttnRemoveAppendix.presence = "visible";	
				
				//Set button so that it also does not show on print
				objNode.sfFormHeader.bttnRemoveAppendix.relevant = "-print";				
			}
		}
	}
}


