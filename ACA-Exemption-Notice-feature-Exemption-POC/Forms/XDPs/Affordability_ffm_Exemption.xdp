/**
 * Validates Appendix C authorized representative section of form
 *  @param - objNode - form node to evaluate
 *  @return - If valid, returns true; otherwise, false
 */
function validateAppendixC_AuthorizedRepAssist(objNode)
{
	if ( objNode.sfRepresentative.sfLegalName.fldFirstName.rawValue != "" &amp;&amp; objNode.sfRepresentative.sfLegalName.fldFirstName.rawValue != null )
	{
		//Check for last name
		if ( soValidations.checkReqFieldIsBlank(objNode.sfRepresentative.sfLegalName.fldLastName, "") )
		{
			soValidations.addError(objNode.sfRepresentative.sfLegalName.fldLastName, "APPENDIX C - Authorized Representative Section", "- Last name is empty");
			soValidations.setFieldErrorState(objNode.sfRepresentative.sfLegalName.fldLastName, true);
		}
	
	
		var emptyAddress = soValidations.areTheseFieldsEmpty([objNode.sfRepresentative.fldStreetAddress, objNode.sfRepresentative.fldAptNo,
																objNode.sfRepresentative.fldCity, objNode.sfRepresentative.fldState,
																objNode.sfRepresentative.fldZip]);

