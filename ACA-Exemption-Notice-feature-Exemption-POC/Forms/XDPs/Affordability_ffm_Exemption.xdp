function setOtherIncomeFields(strAccess)
{
	var vItems = sfOtherIncomeDetail.sfIncomes.resolveNodes("sfIncome[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		if ( strAccess == "open" )
		{
			vItems.item(i).chbType.access = strAccess;
			vItems.item(i).chbType.execEvent("change");
		}
		else
		{
			//Close all fields
			vItems.item(i).chbType.access = strAccess;
			vItems.item(i).fldAmount.access = strAccess;
			vItems.item(i).lstHowOften.access = strAccess;
			vItems.item(i).fldIncomeDateStart.access = strAccess;
