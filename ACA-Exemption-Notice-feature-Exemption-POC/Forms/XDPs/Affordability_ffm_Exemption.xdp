{
	setDeductionFields("readOnly");
	soExemption.clearAllDeductions(this.parent.parent);	
}
else
{
	setDeductionFields("open");
}


function setDeductionFields(strAccess)
{
	var vItems = sfDeductions.resolveNodes("sfDeduction[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		if ( strAccess == "open" )
		{
			vItems.item(i).chbType.access = strAccess;
			vItems.item(i).chbType.execEvent("change");
		}
		else
		{
			//Close all fields	
			vItems.item(i).chbType.access = strAccess;
			vItems.item(i).fldYearlyAmount.access = strAccess;
			vItems.item(i).rdoDeductions.access = strAccess;
			
			//If last, close other field type		
			if ( i == (vItems.length - 1) )
				vItems.item(i).fldTypeOther.access = strAccess;				
		}
	}
}
</script>
