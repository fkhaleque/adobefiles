	if ( validateStep0(xfa.resolveNode("Page1.pgStep1.sfStep1[0]")) &amp;&amp;
			Page1.pgStep1.sfStep1.sfStep1Header.sfQuestionSect1.sfQuestionSect1a.rdoFutureExemption.rawValue != "N" &amp;&amp;
				Page1.pgStep1.sfStep1.sfStep1Header.sfQuestionSect1.sfQuestionSect1b.sfPlanToFile.rdoPlanToFile.rawValue != "N" )
	{
		//Validate Step 1 - Personal Information
		validateStep1(xfa.resolveNode("Page1.pgStep1.sfStep1[0]"));
		
		//Validate Step 2 - First person
		validateStep2Person(xfa.resolveNode("Page1.pgStep2Person1.sfPerson1[0]"), 1);
	
		//Validate Step 2 if pgStep2Person2 is visible - All Person 2s
		var intStep2PeopleCount = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s").instanceManager.count;
		var objPage = xfa.resolveNode("Page1.pgStep2Person2");	
	
		if ( intStep2PeopleCount &gt; 0 &amp;&amp; objPage.presence != "hidden" )
		{	
			for( var i=0; i&lt;intStep2PeopleCount; i++ )
			{
				objNode = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s[" + i + "]");
				//app.alert("objNode: " + objNode.somExpression);
				validateStep2Person(objNode, i + 2);
			}
		}
	
		//Validate Step 4 - Read &amp; Sign This Application		
		validateStep4(xfa.resolveNode("Page1.pgStep4and5[0]"));
	
		//Validate minimum count of expectec Appendix As - SUPPRESSED: Minimum count can't be effectively determined
		//var intAppendixAMinCount = validateExpectedAppendixACount();
		
		//Validate Appendix As if pgAppendixA is visible
		var intAppendCount = xfa.resolveNode("Page1.pgAppendixA.sfAppendixAs").instanceManager.count;
		var objPage = xfa.resolveNode("Page1.pgAppendixA");	
	
		if ( intAppendCount &gt; 0 &amp;&amp; objPage.presence != "hidden" )
		{	
			for( var i=0; i&lt;intAppendCount; i++ )
			{
				objNode = xfa.resolveNode("Page1.pgAppendixA.sfAppendixAs[" + i + "]");
				//app.alert("objNode: " + objNode.somExpression);
				validateAppendixA(objNode, i + 1);
			}
		}
		
		//Validate Appendix C agent, nav, counselor and broker assistance section of form
		validateAppendixC_CounselorAssist(xfa.resolveNode("Page1.pgAppendixC[0]"));
		
		//Validate Appendix C authorized representative section of form
		validateAppendixC_AuthorizedRepAssist(xfa.resolveNode("Page1.pgAppendixC[0]"));
	}
	
	return soValidations.isValid();
}


/**
 * Validates Step 0 pages (preliminary questions)
 *  @param - objNode - form node to evaluate
 *  @return - If valid, returns true; otherwise, false
 */
function validateStep0(objNode)
{	
	if ( soValidations.checkReqFieldIsBlank(objNode.sfStep1Header.sfQuestionSect1.sfQuestionSect1a.rdoFutureExemption, "") )
    {
        soValidations.addError(objNode.sfStep1Header.sfQuestionSect1.sfQuestionSect1a.rdoFutureExemption, "STEP 1", "- You must select if this is for a future exemption");
    }
    else if ( objNode.sfStep1Header.sfQuestionSect1.sfQuestionSect1a.rdoFutureExemption.rawValue != "N" )
    {
	    if ( soValidations.checkReqFieldIsBlank(objNode.sfStep1Header.sfQuestionSect1.sfQuestionSect1b.sfPlanToFile.rdoPlanToFile, "") )
	    {
	        soValidations.addError(objNode.sfStep1Header.sfQuestionSect1.sfQuestionSect1b.sfPlanToFile.rdoPlanToFile, "STEP 1", "- You must indicate if you are a resident in one of the locations presented");
	    }
	}
	
	return soValidations.isValid();	
}


/**
 * Validates Step 1 pages
 *  @param - objNode - form node to evaluate
 *  @return - If valid, returns true; otherwise, false
 */
function validateStep1(objNode)
{	
	/*
	 * Check for first and last names
	 */	   
	if ( soValidations.checkReqFieldIsBlank(objNode.sfLegalName.sfName.fldFirstName, "") )
	{	
		soValidations.addError(objNode.sfLegalName.sfName.fldFirstName, "STEP 1", "- First name is empty");
	}
	
	if ( soValidations.checkReqFieldIsBlank(objNode.sfLegalName.sfName.fldLastName, "") ) 
	{
		soValidations.addError(objNode.sfLegalName.sfName.fldLastName, "STEP 1", "- Last name is empty");
	}
	

	/*
	 * Check for valid addresses
	 */
	var homeEmpty = soValidations.areTheseFieldsEmpty([objNode.sfHomeAddress.fldStreetAddress, objNode.sfHomeAddress.fldAptNo,
													objNode.sfHomeAddress.fldCity, objNode.sfHomeAddress.fldState,
													objNode.sfHomeAddress.fldZip, objNode.sfHomeAddress.fldCounty]);
	
	var mailEmpty = soValidations.areTheseFieldsEmpty([objNode.sfHomeAddress.fldStreetAddress, objNode.sfHomeAddress.fldAptNo,
													objNode.sfHomeAddress.fldCity, objNode.sfHomeAddress.fldState,
													objNode.sfHomeAddress.fldZip, objNode.sfHomeAddress.fldCounty]);
	
	var homeValid = soValidations.isAddressValid(objNode.sfHomeAddress.fldStreetAddress.rawValue,
													objNode.sfHomeAddress.fldAptNo.rawValue,
													objNode.sfHomeAddress.fldCity.rawValue,
													objNode.sfHomeAddress.fldState.rawValue,
													objNode.sfHomeAddress.fldZip.rawValue,
													objNode.sfHomeAddress.fldCounty.rawValue,
													false);
	
	var mailValid =  soValidations.isAddressValid(objNode.sfMailingAddress.fldStreetAddress.rawValue, 
														objNode.sfMailingAddress.fldAptNo.rawValue,
														objNode.sfMailingAddress.fldCity.rawValue,
														objNode.sfMailingAddress.fldState.rawValue,
														objNode.sfMailingAddress.fldZip.rawValue,
														objNode.sfMailingAddress.fldCounty.rawValue,
														false);
	
	//Check if both are invalid
	if ( !homeValid &amp;&amp; !mailValid )
	{
		//At least one of them must be specified.
		//Re-run the validations, this time, highlighting the broken fields
		soValidations.isValidAddress(objNode.sfHomeAddress.fldStreetAddress, 
									objNode.sfHomeAddress.fldAptNo,
									objNode.sfHomeAddress.fldCity,
									objNode.sfHomeAddress.fldState,
									objNode.sfHomeAddress.fldZip,
									objNode.sfHomeAddress.fldCounty,
									true);
	
		soValidations.isValidAddress(objNode.sfMailingAddress.fldStreetAddress, 
									objNode.sfMailingAddress.fldAptNo,
									objNode.sfMailingAddress.fldCity,
									objNode.sfMailingAddress.fldState,
									objNode.sfMailingAddress.fldZip,
									objNode.sfMailingAddress.fldCounty,
									true);
									
		soValidations.addError(objNode.sfHomeAddress.fldStreetAddress, "STEP 1", "- Must specify a valid home or mailing address");
	
	}


	/*
	 * Check for at least one valid phone number
	 */
	var emptyPhone = soValidations.areTheseFieldsEmpty([objNode.sfPhoneNumbers.fldPhoneArea, objNode.sfPhoneNumbers.fldPhonePrefix, objNode.sfPhoneNumbers.fldPhoneSuffix]);
	var emptyPhoneOther = soValidations.areTheseFieldsEmpty([objNode.sfPhoneNumbers.fldPhoneOtherArea, objNode.sfPhoneNumbers.fldPhoneOtherPrefix, objNode.sfPhoneNumbers.fldPhoneOtherSuffix]);
	var errPhone = false;
	var errPhoneOther = false;
	
	if ( emptyPhone &amp;&amp; emptyPhoneOther )
	{
		//No phone number are filled
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneArea, "STEP 1", "- Must specify at least one valid telephone number");
		errPhone = true;
		errPhoneOther = true;
	}
	else
	{	
		var validPhone = soValidations.concatAndValidatePhone(objNode.sfPhoneNumbers.fldPhoneArea, objNode.sfPhoneNumbers.fldPhonePrefix, objNode.sfPhoneNumbers.fldPhoneSuffix);
		var validPhoneOther = soValidations.concatAndValidatePhone(objNode.sfPhoneNumbers.fldPhoneOtherArea, objNode.sfPhoneNumbers.fldPhoneOtherPrefix, objNode.sfPhoneNumbers.fldPhoneOtherSuffix);	
		
		if ( false == ( ( validPhone &amp;&amp; validPhoneOther ) || ( validPhone &amp;&amp; emptyPhoneOther ) || ( emptyPhone &amp;&amp; validPhoneOther ) ) )
		{
			//Filled, but it's not valid
			if ( !validPhone &amp;&amp; emptyPhoneOther )
			{
				//Phone is invalid, but Other Phone is not specified
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneArea, "STEP 1", "- Phone number is invalid");	
				errPhone = true;
			}
			else if ( !validPhoneOther &amp;&amp; emptyPhone )
			{
				//Other Phone is is invalid, but Phone is not specified
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherArea, "STEP 1", "- Other phone number is invalid");	
				errPhoneOther = true;
			}
			else
			{
				//Both are invalid
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneArea, "STEP 1", "- Phone number is invalid");	
				soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherArea, "STEP 1", "- Other phone number is invalid");
				errPhone = true;
				errPhoneOther = true;
			}
		}
	}
	
	if ( errPhone )
	{
		soValidations.addError(objNode.sfPhoneNumbers.fldPhonePrefix, "STEP 1", null);	
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneSuffix, "STEP 1", null);	
	}
	
	if ( errPhoneOther )
	{
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherPrefix, "STEP 1", null);	
		soValidations.addError(objNode.sfPhoneNumbers.fldPhoneOtherSuffix, "STEP 1", null);		
	}
		
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneArea, errPhone);
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhonePrefix, errPhone);	
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneSuffix, errPhone);
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneOtherArea, errPhoneOther);
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneOtherPrefix, errPhoneOther);	
	soValidations.setFieldErrorState(objNode.sfPhoneNumbers.fldPhoneOtherSuffix, errPhoneOther);


	/*
	 * Check for valid e-mail
	 */
	var validEmail = soValidations.isValidEmail(objNode.sfEmailAddress.fldEmailAddress.rawValue) 
					|| ( objNode.sfMarketplaceInfo.rdoMarketplaceInfo.rawValue != "Y" &amp;&amp; objNode.sfEmailAddress.fldEmailAddress.rawValue == null );
	
	soValidations.setFieldErrorState (objNode.sfEmailAddress.fldEmailAddress, !validEmail);
		
	if ( !validEmail )
	{
		soValidations.addError(objNode.sfEmailAddress.fldEmailAddress, "STEP 1", "- Email address is invalid");
	}
		
	return soValidations.isValid();
}


/**
 * Validates Step 2 pages for all people
 *  @param - objNode - form node to evaluate
 *  @param - intPerson - person number to validate
 *  @return - If valid, returns true; otherwise, false
 */
function validateStep2Person(objNode, intPerson)
{
	//Process Person1 AND Person2s nodes only if pgStep2Person2 page is visible
	if ( intPerson == 1 || ( intPerson &gt; 1 &amp;&amp; objNode.parent.presence != "hidden" ) )
	{
		//Clear error states
		soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBYear, false);
		soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBDay, false);	
		soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBMonth, false);
		soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN1, false);
		soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN2, false);
		soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN3, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.rdoPlanToFile, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.rdoFileJointly, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.fldSpouse, false);
		soValidations.setFieldErrorState(objNode.sfTaxes.flddependents, false);
		soValidations.setFieldErrorState(objNode.sfWantExemption.rdoWantExemption, false);
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect1.rdoPregnant, false);
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect1.fldNumberofBabies, false);
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect4.rdoFosterCare, false);		
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect5.rdoUseTobacco, false);
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect6.rdoUSCitizen, false);
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect7.rdoDerivedCitizen, false);
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect8.fldCertifgicateNumber, false);
		soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect8.fldAlienNumber, false);
		soValidations.setFieldErrorState(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect1.rdoEnrolledinHealthCoverage, false); 
		soValidations.setFieldErrorState(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect4.rdoJobCoverage, false);
		soValidations.setFieldErrorState(objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome, false);
		
	
		//Person 1 has a list box for number of dependents while Person &gt;1 has a yes / no checkbox
		if ( intPerson == 1 )
			soValidations.setFieldErrorState(objNode.sfTaxes.listDependents, false);
		else
			soValidations.setFieldErrorState(objNode.sfTaxes.rdoClaimdependents, false);
	
		//Check if Person 2 first name is filled if 6a in Step 2 is yes (spouse name is required)
		if ( intPerson == 2 )
		{
			if ( Page1.pgStep2Person1.sfPerson1.sfTaxes.rdoFileJointly.rawValue == "Y" &amp;&amp;
					( objNode.sfStepHeader.sfPerson.sfName.fldFirstName.rawValue == "" || objNode.sfStepHeader.sfPerson.sfName.fldFirstName.rawValue == null ) )
			{
				soValidations.addError(objNode.sfStepHeader.sfPerson.sfName.fldFirstName, "STEP 2 - PERSON " + intPerson, "- Spouse's first name is required if filing jointly");
				soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldFirstName, true);
			}
		}
		
		if ( intPerson &gt; 1 )
		{
			//Name for intPerson = 1 is mirrored from Step 1 into a locked field
			
			if ( soValidations.checkReqFieldIsBlank(objNode.sfStepHeader.sfPerson.sfName.fldFirstName, "") )
			{	
				soValidations.addError(objNode.sfStepHeader.sfPerson.sfName.fldFirstName, "STEP 2 - PERSON " + intPerson, "- First name is empty");
				soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldFirstName, true);
			}
			
			if ( soValidations.checkReqFieldIsBlank(objNode.sfStepHeader.sfPerson.sfName.fldLastName, "") ) 
			{
				soValidations.addError(objNode.sfStepHeader.sfPerson.sfName.fldLastName,  "STEP 2 - PERSON " + intPerson, "- Last name is empty");
				soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldLastName, true);		
			}
		}
						
		//Check for validity of DOB or SSN
		var validDOB = soValidations.isValidDate(objNode.sfStepHeader.sfPerson.sfName.fldDOBYear.rawValue, objNode.sfStepHeader.sfPerson.sfName.fldDOBMonth.rawValue, objNode.sfStepHeader.sfPerson.sfName.fldDOBDay.rawValue);
		var validSSN = soValidations.isValidSSN(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN1.editValue + objNode.sfStepHeader.sfPerson.sfSSN.fldSSN2.editValue + objNode.sfStepHeader.sfPerson.sfSSN.fldSSN3.editValue);
	
		if ( !validDOB )
		{	
			soValidations.addError(objNode.sfStepHeader.sfPerson.sfName.fldDOBMonth, "STEP 2 - PERSON " + intPerson, "- Date of Birth is not valid");
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBYear, true);
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBDay, true);	
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBMonth, true);
		}

		//SS# is no longer required
		/*		
		if ( !validSSN )
		{	
			soValidations.addError(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN1, "STEP 2 - PERSON " + intPerson, "- Social Security Number is not valid");
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN1, true);
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN2, true);
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN3, true);
		}
		*/	
		
		if ( !validDOB &amp;&amp; !validSSN )   //At least one must be valid
		{
			soValidations.addError(objNode.sfStepHeader.sfPerson.sfName.fldDOBMonth, "STEP 2 - PERSON " + intPerson, "- Valid Date of Birth or Social Security Number must be specified");
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBYear, true);
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBDay, true);	
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfName.fldDOBMonth, true);
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN1, true);
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN2, true);
			soValidations.setFieldErrorState(objNode.sfStepHeader.sfPerson.sfSSN.fldSSN3, true);
		}
	
		// planning to file?
		switch (objNode.sfTaxes.rdoPlanToFile.rawValue)
		{
			case null:  //Did not specify
			case "":
				soValidations.addError(objNode.sfTaxes.rdoPlanToFile, "STEP 2 - PERSON " + intPerson, "- Must indicate if person plans to file a federal income tax return");
				soValidations.setFieldErrorState(objNode.sfTaxes.rdoPlanToFile, true);
				break;
			
			case "Y":  //Yes is filing
		
				switch (objNode.sfTaxes.rdoFileJointly.rawValue)
				{
					case null: //Must specify jointly
					case "":
						soValidations.addError(objNode.sfTaxes.rdoFileJointly, "STEP 2 - PERSON " + intPerson, "- Must indicate if you plan to file jointly with a spouse");
			    		soValidations.setFieldErrorState(objNode.sfTaxes.rdoFileJointly, true);
			    		break;
			    		
			    	case "Y": //Has spouse
	
			    		//Check for spouse name
			    		if ( soValidations.checkReqFieldIsBlank(objNode.sfTaxes.fldSpouse, "") ) 
			    		{		    		
			    			soValidations.addError(objNode.sfTaxes.fldSpouse, "STEP 2 - PERSON " + intPerson, "- Spouse's name is missing");
			    			soValidations.setFieldErrorState(objNode.sfTaxes.fldSpouse, true);
			    		}
			    		break;
			    }
			    
			    //Person 1 has a list box for number of dependents while Person &gt;1 has a yes / no checkbox
			    if ( intPerson == 1 )
			    {
					switch (objNode.sfTaxes.listDependents.rawValue)
					{
						case null: //Must specify number of dependents
						case "":
							soValidations.addError(objNode.sfTaxes.listDependents, "STEP 2 - PERSON " + intPerson, "- Must indicate if you will claim any dependents and how many");
				    		soValidations.setFieldErrorState(objNode.sfTaxes.listDependents, true);
				    		break;
						
						case "0":  //No dependents
				    		break;			    		
				    		
				    	default: //Has dependents
	
				    		//Check for dependent names
				    		if ( soValidations.checkReqFieldIsBlank(objNode.sfTaxes.flddependents, "") ) 
				    		{
				    			soValidations.addError(objNode.sfTaxes.flddependents, "STEP 2 - PERSON " + intPerson, "- Dependent name(s) are missing");
				    			soValidations.setFieldErrorState(objNode.sfTaxes.flddependents, true);
				    		}
				    		break;
					}
			    }
			    else
			    {
					switch (objNode.sfTaxes.rdoClaimdependents.rawValue)
					{
						case null: //Must specify claimed dependents
						case "":
							soValidations.addError(objNode.sfTaxes.rdoClaimdependents, "STEP 2 - PERSON " + intPerson, "- Must indicate if you will claim any dependents");
				    		soValidations.setFieldErrorState(objNode.sfTaxes.rdoClaimdependents, true);
				    		break;
				    		
				    	case "Y": //Has dependents
				    		
				    		//Check for dependent names
				    		if (soValidations.checkReqFieldIsBlank(objNode.sfTaxes.flddependents, "")) 
				    		{
				    			soValidations.addError(objNode.sfTaxes.flddependents, "STEP 2 - PERSON " + intPerson, "- Dependent name(s) are missing");
				    			soValidations.setFieldErrorState(objNode.sfTaxes.flddependents, true);
				    		}
				    		break;
					}
				}
		}
	
		if ( soValidations.checkReqFieldIsBlank(objNode.sfWantExemption.rdoWantExemption, "") )
		{
			soValidations.addError(objNode.sfWantExemption.rdoWantExemption, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No whether you want the exemption");
			soValidations.setFieldErrorState(objNode.sfWantExemption.rdoWantExemption, true);
		}		

		if ( intPerson &gt; 1 )
		{
			if ( soValidations.checkReqFieldIsBlank(objNode.sfTaxes.rdoOnP1Taxes, "") )
			{
				soValidations.addError(objNode.sfTaxes.rdoOnP1Taxes, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No whether you'll be claimed as a dependent for Person 1");
				soValidations.setFieldErrorState(objNode.sfTaxes.rdoOnP1Taxes, true);
			}
			else if ( objNode.sfTaxes.rdoOnP1Taxes.rawValue == "Y" )
			{
				if ( soValidations.checkReqFieldIsBlank(objNode.sfTaxes.fldFiler, "") )
				{
					soValidations.addError(objNode.sfTaxes.fldFiler, "STEP 2 - PERSON " + intPerson, "- Must indicate name of tax filer");
					soValidations.setFieldErrorState(objNode.sfTaxes.fldFiler, true);
				}
				
				if ( soValidations.checkReqFieldIsBlank(objNode.sfTaxes.fldRelToFiler, "") )
				{
					soValidations.addError(objNode.sfTaxes.fldRelToFiler, "STEP 2 - PERSON " + intPerson, "- Must indicate relation to tax filer");
					soValidations.setFieldErrorState(objNode.sfTaxes.fldRelToFiler, true);
				}
			}
		}
		
		/**** Added in 25/11/2015                            ****
		*****                                                ****
		***** Validations for Step2Person1 and Step2Person 2 ****
		*****                                                ****/
		
		
		//Validation Question "Are you pregnant" (P1:Q8 | PX:Q9)
		if ( objNode.sfStepHeader.sfPerson.sfName.rdoSex.rawValue == "F" &amp;&amp;
			soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect1.rdoPregnant, "") )
		{
			soValidations.addError(objNode.sfMiscQuestions.sfQuestSect1.rdoPregnant, "STEP 2 - PERSON " + intPerson, "- As a female, you must specify whether or not you are pregnant");
			soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect1.rdoPregnant, true);	
		}
		
		
		//Validation Question "Number of babies" (P1:Q8a | PX:Q9a)
		if ( objNode.sfMiscQuestions.sfQuestSect1.rdoPregnant.rawValue == "Y" &amp;&amp;
			soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect1.fldNumberofBabies, "") )
		{
			soValidations.addError(objNode.sfMiscQuestions.sfQuestSect1.fldNumberofBabies, "STEP 2 - PERSON " + intPerson, "- As pregnant, you must specify how many babies are expected during pregnancy");
			soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect1.fldNumberofBabies, true);	
		}		
		
		
		/* Validation Question "Were you in foster care" (P1:Q11 | PX:Q12)
		if ( soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect4.rdoFosterCare, "") )
		{
			soValidations.addError(objNode.sfMiscQuestions.sfQuestSect4.rdoFosterCare, "STEP 2 - PERSON " + intPerson, "- Must indicate if you were in foster care at age 18 or older");
			soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect4.rdoFosterCare, true);
		}
		*/
		
		
		//Validation Question "Tabacco use" (P1:Q12 | PX:Q13)
		if ( soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect5.rdoUseTobacco, "") )
		{
			soValidations.addError(objNode.sfMiscQuestions.sfQuestSect5.rdoUseTobacco, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No whether you have used tobacco, 4 or more times per week, in the last 6 months");
			soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect5.rdoUseTobacco, true);
		}
		
		
		//Validation Question "US citizen" (P1:Q13 | PX:Q14)
		if ( soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect6.rdoUSCitizen, "") )
		{
			soValidations.addError(objNode.sfMiscQuestions.sfQuestSect6.rdoUSCitizen, "STEP 2 - PERSON " + intPerson, "- Must indicate whether you are a US citizen");
			soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect6.rdoUSCitizen, true);
		}
		
		
		//Validation Question "Nationalized citizen" (P1:Q14 | PX:Q15)
		if ( objNode.sfMiscQuestions.sfQuestSect6.rdoUSCitizen.rawValue == "N")
		{
			if ( soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect7.rdoDerivedCitizen, "") )
			{
				soValidations.addError(objNode.sfMiscQuestions.sfQuestSect7.rdoDerivedCitizen, "STEP 2 - PERSON " + intPerson, "- Must indicate whether you are a Naturalized or Derived citizen");
				soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect7.rdoDerivedCitizen, true);
			}			
		}
	
	
		//Validation for Question "Alien # or Cert #" (P1:Q14a &amp; b | PX:Q15a &amp; b)
		if ( objNode.sfMiscQuestions.sfQuestSect7.rdoDerivedCitizen.rawValue == "Y" )
		{
			if ( soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect8.fldAlienNumber, "") )
			{
				soValidations.addError(objNode.sfMiscQuestions.sfQuestSect8.fldAlienNumber, "STEP 2 - PERSON " + intPerson, "- Must provide your Alien Number");
				soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect8.fldAlienNumber, true);
			}
			
			if ( soValidations.checkReqFieldIsBlank(objNode.sfMiscQuestions.sfQuestSect8.fldCertifgicateNumber, "") )
			{
				soValidations.addError(objNode.sfMiscQuestions.sfQuestSect8.fldCertifgicateNumber, "STEP 2 - PERSON " + intPerson, "- Must provide your Certificate Number");
				soValidations.setFieldErrorState(objNode.sfMiscQuestions.sfQuestSect8.fldCertifgicateNumber, true);
			}
		}
		
		
		//Validation for Question "Enrolled in health coverage" (P1:Q18 | PX:Q19)
		if ( soValidations.checkReqFieldIsBlank(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect1.rdoEnrolledinHealthCoverage, "") )
		{
			soValidations.addError(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect1.rdoEnrolledinHealthCoverage, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No to indicate whether you are now enrolled in health coverage");
			soValidations.setFieldErrorState(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect1.rdoEnrolledinHealthCoverage, true);
		}
		
		
		//Validation for Question "Offered health coverage via job" (P1:Q19 | PX:Q20)
		if ( soValidations.checkReqFieldIsBlank(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect4.rdoJobCoverage, "") )
		{
			soValidations.addError(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect4.rdoJobCoverage, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No to indicate if you are offered health coverage from a job");
			soValidations.setFieldErrorState(objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect4.rdoJobCoverage, true);
		}
		
		
		//Validation for Question "Receive income this year" (P1:Q21 | PX:Q22)
		if ( soValidations.checkReqFieldIsBlank(objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome, "") )
		{
			soValidations.addError(objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome, "STEP 2 - PERSON " + intPerson, "- Must select Yes or No to indicate if you are receiving any income during any month this year");
			soValidations.setFieldErrorState(objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome, true);
		}
		

		//Validation for Question "Receive income from a job this year" (P1:Q21 | PX:Q22)
		if ( soValidations.checkReqFieldIsBlank(objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome, "") )
		{
			soValidations.addError(objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome, "STEP 2 - PERSON " + intPerson, "- You must select whether you will receive income from a job this year");
			soValidations.setFieldErrorState(objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome, true);
		}
		else
		{
			if ( objNode.sfIncome.sfIncomeTitle.sfIncomeThisYear.rdoReceiveIncome.rawValue == "Y" )
			{
				//Validate job section
				var intCountJobs = validateJobsSection(objNode, "STEP 2 - PERSON " + intPerson);
				//app.alert("intCountJobs: " + intCountJobs);
			}
		}

		
		//Validation for Question "Other income" (P1:Q28 | PX:Q29)
		if ( objNode.sfOtherIncome.sfFlowed.sfTitle.chbNoIncome.rawValue != "Y" )
		{
			//Validate Other Income section
			var intCountOtherIncome = validateOtherIncome(objNode, "STEP 2 - PERSON " + intPerson);
			//app.alert("intCountOtherIncome: " + intCountOtherIncome);
			
			if ( intCountOtherIncome == 0 )
			{
				soValidations.addError(objNode.sfOtherIncome.sfFlowed.sfTitle.chbNoIncome, "STEP 2 - PERSON " + intPerson, "- At least one other type of income needs to be specified");				
				soValidations.setFieldErrorState(objNode.sfOtherIncome.sfFlowed.sfTitle.chbNoIncome, true);				
			}
		}


		//Validation for Question "Deductions" (P1:Q29 | PX:Q30)
		if ( objNode.sfOtherIncome.sfFlowed.sfDeductionDetail.sfInstructions.chbNoDeductions.rawValue != "Y" )
		{
			//Validate Deductions section
			var intCountDeductions = validateDeductions(objNode, "STEP 2 - PERSON " + intPerson);	
			//app.alert("intCountDeductions: " + intCountDeductions);
			
			if ( intCountDeductions == 0 )
			{
				soValidations.addError(objNode.sfOtherIncome.sfFlowed.sfDeductionDetail.sfInstructions.chbNoDeductions, "STEP 2 - PERSON " + intPerson, "- At least one type of deduction needs to be specified");				
				soValidations.setFieldErrorState(objNode.sfOtherIncome.sfFlowed.sfDeductionDetail.sfInstructions.chbNoDeductions, true);				
			}						
		}


		//Validate that other ethinicity is specified
		if ( objNode.sfEthnicityAndRace.sfEthnicities.resolveNode("sfEthnic[5]").chkEthnic.rawValue == 6 &amp;&amp;
				( objNode.sfEthnicityAndRace.sfEthnicities.fldOtherEthnicity.rawValue == "" ||
					objNode.sfEthnicityAndRace.sfEthnicities.fldOtherEthnicity.rawValue == null ) )
		{
			soValidations.addError(objNode.sfEthnicityAndRace.sfEthnicities.fldOtherEthnicity, "STEP 2 - PERSON " + intPerson, "- You must specify your other ethnicity");
			soValidations.setFieldErrorState(objNode.sfEthnicityAndRace.sfEthnicities.fldOtherEthnicity, true);	
		}

		//Validate that other race is specified
		if ( objNode.sfEthnicityAndRace.sfRaces.resolveNode("sfRace[14]").chkRace.rawValue == 15 &amp;&amp; 
				( objNode.sfEthnicityAndRace.sfRaces.fldOtherRace.rawValue == "" ||
					objNode.sfEthnicityAndRace.sfRaces.fldOtherRace.rawValue == null ) )
		{
			soValidations.addError(objNode.sfEthnicityAndRace.sfRaces.fldOtherRace, "STEP 2 - PERSON " + intPerson, "- You must specify your other race");
			soValidations.setFieldErrorState(objNode.sfEthnicityAndRace.sfRaces.fldOtherRace, true);	
		}
		
				
		//Validate at least two pages of STEP 2 exist for both spouse and dependent
		if ( intPerson == 1 )
		{
			var intStep2PeopleCount = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s").instanceManager.count;
			
			if ( intStep2PeopleCount &lt; 2 &amp;&amp;
					objNode.sfTaxes.rdoFileJointly.rawValue == "Y" &amp;&amp;
						objNode.sfTaxes.listDependents.rawValue != "None" &amp;&amp;
							objNode.sfTaxes.listDependents.rawValue &gt;= 1 )
			{
				soValidations.addError(objNode.sfTaxes.rdoPlanToFile, "STEP 2 - PERSON " + intPerson, "- At least 2 people in Step 2 need exist for both spouse &amp; dependent");
			}
		}
	}
	
	//Were there issues?
	return soValidations.isValid();

}


/**
 * Validates jobs section
 *  @param - objNode - object node to evaluate
 *  @param - strSect - section in which job is being evaluated
 *  @return - valid job count
 */
function validateJobsSection(objNode, strSect)
{
	var intCountJobs = 0;
	
	var vItems = objNode.sfIncome.sfJobs.resolveNodes("sfJob[*]");	
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		//Increment job count
		intCountJobs++;

		//Clear error states
		soValidations.setFieldErrorState(vItems.item(i).sfSelfEmployed.rdoSelfEmployed, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfEmpBus.fldNmEmpBus, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfIncomeFreq.sfIncome.fldAmtEmpBus, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfIncomeFreq.sfIncome.sfWeeklyAvgHours.fldAverageHours, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfIncomeFreq.sfIncome.radFrequency, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobYear, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobDay, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobMonth, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobMonth, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobDay, false);
		soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobYear, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfIncomeExpected.sfYear1.fldThisYear, false);	
		soValidations.setFieldErrorState(vItems.item(i).sfIncomeExpected.sfYear2.fldNextYear, false);	
	
		if ( soValidations.checkReqFieldIsBlank(vItems.item(i).sfSelfEmployed.rdoSelfEmployed, "") )
		{
			soValidations.addError(vItems.item(i).sfSelfEmployed.rdoSelfEmployed, strSect, "- Must indicate if you are self employed for Job " + intCountJobs);
			soValidations.setFieldErrorState(vItems.item(i).sfSelfEmployed.rdoSelfEmployed, true);
		}
		
		if ( soValidations.checkReqFieldIsBlank(vItems.item(i).sfEmpBus.fldNmEmpBus, "") )
		{
			soValidations.addError(vItems.item(i).sfEmpBus.fldNmEmpBus, strSect, "- Must enter your Employer's Name for Job " + intCountJobs);
			soValidations.setFieldErrorState(vItems.item(i).sfEmpBus.fldNmEmpBus, true);
		}
		
		if ( soValidations.checkReqFieldIsBlank(vItems.item(i).sfIncomeFreq.sfIncome.fldAmtEmpBus, "") )
		{
			soValidations.addError(vItems.item(i).sfIncomeFreq.sfIncome.fldAmtEmpBus, strSect, "- Must enter your Income amount for Job " + intCountJobs);
			soValidations.setFieldErrorState(vItems.item(i).sfIncomeFreq.sfIncome.fldAmtEmpBus, true);
		}
		
		//If not self-employed, average hours are required
		if ( vItems.item(i).sfSelfEmployed.rdoSelfEmployed.rawValue == "N" &amp;&amp; 
				( vItems.item(i).sfIncomeFreq.sfIncome.sfWeeklyAvgHours.fldAverageHours.rawValue == "" ||
					vItems.item(i).sfIncomeFreq.sfIncome.sfWeeklyAvgHours.fldAverageHours.rawValue == null ) )
		{
			soValidations.addError(vItems.item(i).sfIncomeFreq.sfIncome.sfWeeklyAvgHours.fldAverageHours, strSect, "- Must enter your average hours worked each week for Job " + intCountJobs);
			soValidations.setFieldErrorState(vItems.item(i).sfIncomeFreq.sfIncome.sfWeeklyAvgHours.fldAverageHours, true);
		}
		
		if ( soValidations.checkReqFieldIsBlank(vItems.item(i).sfIncomeFreq.sfIncome.radFrequency, "") )
		{
			soValidations.addError(vItems.item(i).sfIncomeFreq.sfIncome.radFrequency, strSect, "- Must enter how often you will be receiving this wage for Job " + intCountJobs);
			soValidations.setFieldErrorState(vItems.item(i).sfIncomeFreq.sfIncome.radFrequency, true);
		}
		
		if ( soValidations.checkReqFieldIsBlank(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobMonth, "") &amp;&amp;
				soValidations.checkReqFieldIsBlank(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobDay, "") &amp;&amp;		
					soValidations.checkReqFieldIsBlank(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobYear, "") )
		{
			soValidations.addError(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobMonth, strSect, "- Must enter the date you started for Job " + intCountJobs);
			soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobMonth, true);
			soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobDay, true);
			soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfStartDt.fldStartJobYear, true);						
		}
		
		if ( vItems.item(i).sfStartEndDates.sfEndDt.chbNoEndDate.rawValue != "Y" )
		{
			if ( soValidations.checkReqFieldIsBlank(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobMonth, "") &amp;&amp;
					soValidations.checkReqFieldIsBlank(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobDay, "") &amp;&amp;
						soValidations.checkReqFieldIsBlank(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobYear, "") )
			{
				soValidations.addError(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobMonth, strSect, "- Must enter the date you ended for Job " + intCountJobs);
				soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobMonth, true);
				soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobDay, true);
				soValidations.setFieldErrorState(vItems.item(i).sfStartEndDates.sfEndDt.fldEndJobYear, true);	
			}
		}
		
		if ( vItems.item(i).sfIncomeExpected.sfYear1.fldThisYear.rawValue != "" &amp;&amp; vItems.item(i).sfIncomeExpected.sfYear1.fldThisYear.rawValue != null )
		{
			if ( vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbJanuary.rawValue != 1 &amp;&amp;
					vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbFebruary.rawValue != 1 &amp;&amp;
						vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbMarch.rawValue != 1 &amp;&amp;
							vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbApril.rawValue != 1 &amp;&amp;
								vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbMay.rawValue != 1 &amp;&amp;
									vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbJune.rawValue != 1 &amp;&amp;
										vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbJuly.rawValue != 1 &amp;&amp;
											vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbAugust.rawValue != 1 &amp;&amp;
												vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbSeptember.rawValue != 1 &amp;&amp;
													vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbOctober.rawValue != 1 &amp;&amp;
														vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbNovember.rawValue != 1 &amp;&amp;
															vItems.item(i).sfIncomeExpected.sfYear1.sfMonths.chbDecember.rawValue != 1 )

			{
				soValidations.addError(vItems.item(i).sfIncomeExpected.sfYear1.fldThisYear, strSect, "- Must specify months you expect to get income this year for Job " + intCountJobs);
				soValidations.setFieldErrorState(vItems.item(i).sfIncomeExpected.sfYear1.fldThisYear, true);				
			}
		}
		
		if ( vItems.item(i).sfIncomeExpected.sfYear2.fldNextYear.rawValue != "" &amp;&amp; vItems.item(i).sfIncomeExpected.sfYear2.fldNextYear.rawValue != null )
		{
			if ( vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbJanuary.rawValue != 1 &amp;&amp;
					vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbFebruary.rawValue != 1 &amp;&amp;
						vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbMarch.rawValue != 1 &amp;&amp;
							vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbApril.rawValue != 1 &amp;&amp;
								vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbMay.rawValue != 1 &amp;&amp;
									vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbJune.rawValue != 1 &amp;&amp;
										vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbJuly.rawValue != 1 &amp;&amp;
											vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbAugust.rawValue != 1 &amp;&amp;
												vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbSeptember.rawValue != 1 &amp;&amp;
													vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbOctober.rawValue != 1 &amp;&amp;
														vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbNovember.rawValue != 1 &amp;&amp;
															vItems.item(i).sfIncomeExpected.sfYear2.sfMonths.chbDecember.rawValue != 1 )

			{
				soValidations.addError(vItems.item(i).sfIncomeExpected.sfYear2.fldNextYear, strSect, "- Must specify months you expect to get income next year for Job " + intCountJobs);
				soValidations.setFieldErrorState(vItems.item(i).sfIncomeExpected.sfYear2.fldNextYear, true);				
			}
		}				
	}

	return intCountJobs;		
}


/**
 * Validates Other Income section
 *  @param - objNode - object node to evaluate
 *  @param - strSect - section in which other income is being evaluated
 *  @return - If valid, returns true; otherwise, false
 */
function validateOtherIncome(objNode, strSect)
{
	var intCountOtherIncome = 0;

	var vItems = objNode.sfOtherIncome.sfFlowed.sfOtherIncomeDetail.sfIncomes.resolveNodes("sfIncome[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		var startDate=null;
		var endDate =null;
	
		//console.println(vItems.item(i).name + " " + i);

		//Clear error states
		soValidations.setFieldErrorState(vItems.item(i).fldAmount, false);
		soValidations.setFieldErrorState(vItems.item(i).lstHowOften, false);
		soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateStart, false);
		soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateEnd, false);
		soValidations.setFieldErrorState(vItems.item(i).chbNoExpectedEnd, false);
		soValidations.setFieldErrorState(vItems.item(i).fldNumMonths, false);	
		if ( i == (vItems.length - 1) )
			soValidations.setFieldErrorState(vItems.item(i).fldTypeOther, false);		
			
		if ( vItems.item(i).fldIncomeDateStart.rawValue != null )
		{
			startDate = new Date(vItems.item(i).fldIncomeDateStart.rawValue.replace(/-/g, "/"));
		}
		if ( vItems.item(i).fldIncomeDateEnd.rawValue != null )
		{
			endDate = new Date(vItems.item(i).fldIncomeDateEnd.rawValue.replace(/-/g, "/"));
		}

		// Wants the type of income ?
		if ( vItems.item(i).chbType.rawValue &gt; 0 )
		{	
			//Increment count
			intCountOtherIncome++;
				
			//If last, clear other field type		
			if ( i == (vItems.length - 1) )
			{
				if ( soValidations.checkReqFieldIsBlank(vItems.item(i).fldTypeOther, "") )
				{
					soValidations.addError(vItems.item(i).fldTypeOther, strSect, "- Must specifiy other income type " + (i+1));
					soValidations.setFieldErrorState(vItems.item(i).fldTypeOther, true);				
				}
			}
			
			//Check income amount
			if ( soValidations.checkReqFieldIsBlank(vItems.item(i).fldAmount, "") )
			{
				soValidations.addError(vItems.item(i).fldAmount, strSect, "- Income amount is missing for other income type " + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).fldAmount, true);
			}
			
			//Check income freqency
			if ( soValidations.checkReqFieldIsBlank(vItems.item(i).lstHowOften, "") )
			{
				soValidations.addError(vItems.item(i).lstHowOften, strSect, "- Income frequency is missing for other income type " + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).lstHowOften, true);
			}
			
			//Check all date valdiation combinations
			if ( startDate == null )
			{
				soValidations.addError(vItems.item(i).fldIncomeDateStart, strSect, "- Start date is required for other income type" + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateStart, true);
			}
			else if ( startDate &lt; 0 )
			{
				soValidations.addError(vItems.item(i).fldIncomeDateStart, strSect, "- Start date is required for other income type" + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateStart, true);
			}
			
			if ( vItems.item(i).chbNoExpectedEnd.rawValue == "Y" )
			{
				//Ongoing - only valid end date is empty
				if ( endDate != null )
				{
					soValidations.addError(vItems.item(i).fldIncomeDateEnd, strSect, "- Both an end date and 'no expected end data' has been specified for other income type " + (i+1) + " - Select one option only");
					soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateEnd, true);
				} 
			}
			else
			{
				if ( endDate == null )
				{
					soValidations.addError(vItems.item(i).fldIncomeDateEnd, strSect, "- End date is required for other income type " + (i+1));
					soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateEnd, true);
				}
				else if ( endDate &lt; 0 )
				{
					soValidations.addError(vItems.item(i).fldIncomeDateEnd, strSect, "- Valid end date is required for other income type " + (i+1));
					soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateEnd, true);
				}
				else
				{
					// do date compare if start date is also specified
					if ( startDate != null )
					{
						if ( startDate.getTime() == endDate.getTime() )
						{
							// dates are same
							soValidations.addError(vItems.item(i).fldIncomeDateEnd, strSect, "- Start date and end date for other income type " + (i+1) + " are the same");
							soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateEnd,true);
						}
						else if ( ( startDate.getTime() - endDate.getTime() ) &gt; 0 )
						{
							// dates are out of order
							soValidations.addError(vItems.item(i).fldIncomeDateEnd, strSect, "- End date for other income type " + (i+1) + " is before the start date");
							soValidations.setFieldErrorState(vItems.item(i).fldIncomeDateEnd, true);
						}
					}
			
				}
			}

			//Check income amount
			if ( soValidations.checkReqFieldIsBlank(vItems.item(i).fldNumMonths, "" ) )
			{
				soValidations.addError(vItems.item(i).fldNumMonths, strSect, "- Number of expected months for income is missing for other income type " + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).fldNumMonths, true);
			}
				
		}
	}
	
	return intCountOtherIncome;
}



/**
 * Validates Deductions section
 *  @param - objNode - object node to evaluate
 *  @param - strSect - section in which deductions is being evaluated
 *  @return - If valid, returns true; otherwise, false
 */
function validateDeductions(objNode, strSect)
{
	var intCountDeductions = 0;

	var vItems = objNode.sfOtherIncome.sfFlowed.sfDeductionDetail.sfDeductions.resolveNodes("sfDeduction[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		//console.println(vItems.item(i).name + " " + i);

		//Clear error states	
		soValidations.setFieldErrorState(vItems.item(i).fldYearlyAmount, false);
		soValidations.setFieldErrorState(vItems.item(i).rdoDeductions, false);
		if ( i == (vItems.length - 1) )
			soValidations.setFieldErrorState(vItems.item(i).fldTypeOther, false);
				
		// Wants the type of income ?
		if ( vItems.item(i).chbType.rawValue &gt; 0 )
		{	
			//Increment count
			intCountDeductions++;
			
			//If last, clear other field type		
			if ( i == (vItems.length - 1) )
			{
				if ( soValidations.checkReqFieldIsBlank(vItems.item(i).fldTypeOther, "") )
				{
					soValidations.addError(vItems.item(i).fldTypeOther, strSect, "- Must specifiy deduction type " + (i+1));
					soValidations.setFieldErrorState(vItems.item(i).fldTypeOther, true);				
				}
			}			
				
			//Check income amount
			if ( soValidations.checkReqFieldIsBlank(vItems.item(i).fldYearlyAmount, "") )
			{
				soValidations.addError(vItems.item(i).fldYearlyAmount, strSect, "- Yearly amount is missing for deduction type " + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).fldYearlyAmount, true);
			}
			
			//Check income freqency
			if ( soValidations.checkReqFieldIsBlank(vItems.item(i).rdoDeductions, "") )
			{
				soValidations.addError(vItems.item(i).rdoDeductions, strSect, "- You must indicate whether you've taken this deduction in the past for deduction type " + (i+1));
				soValidations.setFieldErrorState(vItems.item(i).rdoDeductions, true);
			}					
		}
	}
	
	return intCountDeductions;
}


