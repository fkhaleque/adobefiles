/**
 * Gets starting page relative to all prior pages for barcode step 3 (Yearly Income)
 *  @param - objPage - page in which barcode resides 
 *  @return - start page for barcode
 */
function getRelPgBcodeStep3(objPage)
{
	//Get actual page count and PagePerson2s page count
	intPageActual = xfa.layout.page(objPage);
	intPagePerson2Count = xfa.resolveNodes("data.#pageSet.PagePerson2s[*]").length;
	
	//Continue page number relative to current page - previous PagePerson2s page count
	var intPage = intPageActual - intPagePerson2Count;
	
	//Increment if section PagePerson2s exists
	if ( intPagePerson2Count &gt; 0 )
		intPage++;
	
	return intPage;
}


