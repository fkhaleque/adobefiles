/**
 * Validates the expected minimum count of Appendix A sections in form
 *  @return - Minimum count of people expecting to provide an Appendix A
 */
function validateExpectedAppendixACount()
{
	var intAppendixAMinCount = 0;
	
	if ( Page1.pgStep2Person1.sfPerson1.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect4.rdoJobCoverage.rawValue == "Y" )
		intAppendixAMinCount++;

	//Get expected count for Appendix As for all Person 2s
	var intStep2PeopleCount = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s").instanceManager.count;
	var objPage = xfa.resolveNode("Page1.pgStep2Person2");

	if ( intStep2PeopleCount &gt; 0 &amp;&amp; objPage.presence != "hidden" )
	{	
		for( var i=0; i&lt;intStep2PeopleCount; i++ )
		{
			objNode = xfa.resolveNode("Page1.pgStep2Person2.sfPerson2s[" + i + "]");
			
			if ( objNode.sfOtherHealthCoverage.sfOtherHC.sfOtherHCSect4.rdoJobCoverage.rawValue == "Y" )
				intAppendixAMinCount++;		
		}
	}
	
	var intAppendCount = 0;
	var objPage = xfa.resolveNode("Page1.pgAppendixA");
	
	soValidations.setFieldErrorState(objPage.sfAppendixAs.sfEmployeeInformation.sfEmpInfo.fldFirstName, false);	
	
	if ( objPage.presence != "hidden" )
		intAppendCount = xfa.resolveNode("Page1.pgAppendixA.sfAppendixAs").instanceManager.count;
		
	if ( intAppendCount &lt; intAppendixAMinCount )
	{
		soValidations.addError(objPage.sfAppendixAs.sfEmployeeInformation.sfEmpInfo.fldFirstName, "APPENDIX A - Health Coverage from Jobs Section", "- The number of expected Appendix As does not match minimum count of " + intAppendixAMinCount);
		soValidations.setFieldErrorState(objPage.sfAppendixAs.sfEmployeeInformation.sfEmpInfo.fldFirstName, true);		
	}

	return intAppendixAMinCount;

}


