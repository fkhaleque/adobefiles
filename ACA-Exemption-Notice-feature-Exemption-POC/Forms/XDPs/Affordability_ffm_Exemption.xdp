/**
 * Encodes job data for QR barcode
 *  @param - objJobFormNode - current job form node to encode
 *  @return - encoded string data for job(s) on same page
 */
function generatePageJobsBarcodeString(objJobFormNode)
{
	var strJobsData = " ";
	var intIndex = objJobFormNode.index;

	if ( intIndex == 0 )
	{
		//Skip if first index - first job is encoded in existing barcode on page
		return strJobsData;
	}
	else
	{
		if ( intIndex % 2 != 0 )
		{
			//Process - odd number index barcodes should HAVE data
			strJobsData = generateJobBarcodeString(objJobFormNode);
			
			//Try to process next job on page (if it exists)
			try
			{
				objJobFormNode = objJobFormNode.parent.resolveNode("sfJob[" + (intIndex + 1) + "]");
				
				if ( objJobFormNode != null )
				{
					strJobsData += generateJobBarcodeString(objJobFormNode);
				}
				else
				{
					//Pad-out 2nd (non-existent) job barcode on page for all job fields
					for ( i=1; i&lt;=34; i++ )
					{
						//Pad-out data with empty separators
						strJobsData += ";";
					}
				}
			}
			catch (e)
			{
				//Pad-out 2nd (non-existent) job barcode on page for all job fields
				for ( i=1; i&lt;=34; i++ )
				{
					//Pad-out data with empty separators
					strJobsData += ";";
				}			
			}	
		}
		else
		{
			//Skip - even number index barcodes should have not data on given page
			return strJobsData;
		}
	}
	
	return strJobsData;
}


/**
 * Encodes job data for QR barcode
 *  @param - objJobFormNode - job form node to encode
 *  @return - encoded string data for given job (sfJob[X])
 */
function generateJobBarcodeString(objJobFormNode)
{
	// Concatenate Multi Fields
	
	var strJobStart = objJobFormNode.sfStartEndDates.sfStartDt.fldStartJobMonth.formattedValue
					+ objJobFormNode.sfStartEndDates.sfStartDt.fldStartJobDay.formattedValue
					+ objJobFormNode.sfStartEndDates.sfStartDt.fldStartJobYear.formattedValue;
				
	var strJobEnd = objJobFormNode.sfStartEndDates.sfEndDt.fldEndJobMonth.formattedValue
					+ objJobFormNode.sfStartEndDates.sfEndDt.fldEndJobDay.formattedValue
					+ objJobFormNode.sfStartEndDates.sfEndDt.fldEndJobYear.formattedValue;

	var strJobData = soBarcoding.encodeString(objJobFormNode.sfSelfEmployed.rdoSelfEmployed.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfEmpBus.fldNmEmpBus.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeFreq.sfIncome.fldAmtEmpBus.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeFreq.sfIncome.radFrequency.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeFreq.sfIncome.sfWeeklyAvgHours.fldAverageHours.rawValue)
		+ soBarcoding.encodeString(strJobStart)
		+ soBarcoding.encodeString(strJobEnd)													
		+ soBarcoding.encodeString(objJobFormNode.sfStartEndDates.sfEndDt.chbNoEndDate.rawValue)
		
		+ soBarcoding.encodeString("20" + objJobFormNode.sfIncomeExpected.sfYear1.fldThisYear.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbJanuary.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbFebruary.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbMarch.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbApril.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbMay.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbJune.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbJuly.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbAugust.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbSeptember.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbOctober.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbNovember.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear1.sfMonths.chbDecember.rawValue)
		
		+ soBarcoding.encodeString("20" + objJobFormNode.sfIncomeExpected.sfYear2.fldNextYear.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbJanuary.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbFebruary.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbMarch.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbApril.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbMay.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbJune.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbJuly.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbAugust.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbSeptember.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbOctober.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbNovember.rawValue)
		+ soBarcoding.encodeString(objJobFormNode.sfIncomeExpected.sfYear2.sfMonths.chbDecember.rawValue);		

	//console.println("\n\nSOM: " + objJobFormNode.somExpression + "\n" + strJobData);	

	return strJobData;
}


/**
 * Encodes income data for QR barcode
 *  @param - objIncomesFormNode - income form node to encode
 *  @return - encoded string data for income types
 */
function generateIncomesBarcodeString(objIncomesFormNode)
{
	var strBarcode = "";
	var intSelected = 0;
	var vItems = objIncomesFormNode.resolveNodes("sfIncome[*]");
	
	//console.println("vItems.length: " + vItems.length);

	for( i=0; i&lt;vItems.length; i++ )
	{
		var flgSelected = vItems.item(i).chbType.rawValue;
		
		if ( flgSelected == "1" )
		{
			//Append ID first - return value from encodeNumber containing ";" introdues bug in FOR loop (JS issue)
			strBarcode += soBarcoding.encodeString(i + 1);
			
			strBarcode += soBarcoding.encodeString(vItems.item(i).fldAmount.rawValue)
							+ soBarcoding.encodeString(vItems.item(i).lstHowOften.rawValue)
							+ soBarcoding.encodeDate( soValidations.getDateIsoFormat(vItems.item(i).fldIncomeDateStart) )
							+ soBarcoding.encodeDate( soValidations.getDateIsoFormat(vItems.item(i).fldIncomeDateEnd) )							
							+ soBarcoding.encodeString(vItems.item(i).chbNoExpectedEnd.rawValue)
							+ soBarcoding.encodeString(vItems.item(i).fldNumMonths.rawValue);
			
			intSelected++;
			
			//console.println("strBarcode : " + strBarcode );
		}
		else
		{
			//Append ID first - return blank values
			strBarcode += soBarcoding.encodeString(i + 1);		
			strBarcode += ";;;;;;";
		}
	}
	
	return strBarcode;
}


