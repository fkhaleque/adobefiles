	( soValidations.getEvalSection() == strSectionID || soValidations.getEvalSection() == "" ) )
{
	soValidations.resetErrors();
	soValidations.setEvalSection(strSectionID);	
	
	if ( !soExemption.validateStep2Person(xfa.resolveNode("Page1.pgStep2Person1.sfPerson1[0]"), 1) )
	{
