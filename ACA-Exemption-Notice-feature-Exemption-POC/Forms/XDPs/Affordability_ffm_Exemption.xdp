{
	this.rawValue = soValidations.padDigits(this.rawValue, 2);
	
	var intParentIndex = this.parent.parent.parent.parent.parent.parent.index;
	var objParentData = xfa.datasets.data.data.affordability.resolveNode("person2[" + intParentIndex + "]");
	
	var intIndex = this.parent.parent.parent.index;
	soValidations.saveJobYearBoundData(objParentData.income_coverage.ffm.incomes_jobs.resolveNode("income[" + intIndex + "]").job_months_income_this_yr.job_yr, this);
}
