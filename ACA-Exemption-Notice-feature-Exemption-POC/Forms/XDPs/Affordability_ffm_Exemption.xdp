function validateJobIncomeMultiPartEndDate(oFldStartDtMo, oFldStartDtDay, oFldStartDtYr,
											oFldEndDtMo, oFldEndDtDay, oFldEndDtYr)
{
	if ( ( oFldStartDtMo.rawValue != "" &amp;&amp; oFldStartDtMo.rawValue != null ) &amp;&amp;
			( oFldStartDtDay.rawValue != "" &amp;&amp; oFldStartDtDay.rawValue != null ) &amp;&amp;
				( oFldStartDtYr.rawValue != "" &amp;&amp; oFldStartDtYr.rawValue != null ) &amp;&amp;
					( oFldEndDtMo.rawValue != "" &amp;&amp; oFldEndDtMo.rawValue != null ) &amp;&amp;
						( oFldEndDtDay.rawValue != "" &amp;&amp; oFldEndDtDay.rawValue != null ) &amp;&amp;
							( oFldEndDtYr.rawValue != "" &amp;&amp; oFldEndDtYr.rawValue != null ) )										
	{
		var oStartDt = new Date ( oFldStartDtYr.rawValue, oFldStartDtMo.rawValue - 1, oFldStartDtDay.rawValue );
		//app.alert("Start Date: " + util.printd("mm/dd/yyyy", oStartDt));

		var oEndDt = new Date ( oFldEndDtYr.rawValue, oFldEndDtMo.rawValue - 1, oFldEndDtDay.rawValue );
		//app.alert("End Date: " + util.printd("mm/dd/yyyy", oEndDt))	

		if ( ( oStartDt != null &amp;&amp; oEndDt != null ) &amp;&amp; oEndDt &lt; oStartDt )
		{
			xfa.host.messageBox("Date job ended cannot be before date job started.", soExemption.FORM_NAME,1,0);
			oFldEndDtMo.rawValue = "";
			oFldEndDtDay.rawValue = "";	
			oFldEndDtYr.rawValue = "";					
			xfa.host.setFocus(oFldEndDtMo);
			return false;
		}
	}

	return true;
}


function validateOtherIncomeEndDate(oFldStartDt, oFldEndDt)
{
	if ( ( oFldStartDt.rawValue != "" &amp;&amp; oFldStartDt.rawValue != null ) &amp;&amp;
			( oFldEndDt.rawValue != "" &amp;&amp; oFldEndDt.rawValue != null ) )										
	{
		var arrDate = new Array();

		arrDate = oFldStartDt.formattedValue.split("/");    //oFldStartDtis a form variable of MM/DD/YYYY
		var oStartDt = new Date ( arrDate[2], arrDate[0] - 1, arrDate[1] );
		//app.alert("Start Date: " + util.printd("mm/dd/yyyy", oStartDt));
	
		arrDate = [];
		arrDate = oFldEndDt.formattedValue.split("/"); 
		var oEndDt = new Date ( arrDate[2], arrDate[0] - 1, arrDate[1] );
		//app.alert("End Date: " + util.printd("mm/dd/yyyy", oEndDt))	

		if ( ( oStartDt != null &amp;&amp; oEndDt != null ) &amp;&amp; oEndDt &lt; oStartDt )
		{
			xfa.host.messageBox("Date income ended cannot be before date income started.", soExemption.FORM_NAME,1,0);
			oFldEndDt.rawValue = "";				
			xfa.host.setFocus(oFldEndDt);
			return false;
		}
	}

	return true;
}


