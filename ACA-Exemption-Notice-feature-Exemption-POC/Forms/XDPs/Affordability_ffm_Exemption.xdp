                           <event activity="change" name="event__change">
                              <script contentType="application/x-javascript">
if ( this.rawValue == "Y" )
{
	setOtherIncomeFields("readOnly");
	soExemption.clearAllOtherIncome(sfOtherIncomeDetail);	
}
else
{
	setOtherIncomeFields("open");
}


function setOtherIncomeFields(strAccess)
{
	var vItems = sfOtherIncomeDetail.sfIncomes.resolveNodes("sfIncome[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		if ( strAccess == "open" )
		{
			vItems.item(i).chbType.access = strAccess;
			vItems.item(i).chbType.execEvent("change");
		}
		else
		{
			//Close all fields
			vItems.item(i).chbType.access = strAccess;
			vItems.item(i).fldAmount.access = strAccess;
			vItems.item(i).lstHowOften.access = strAccess;
			vItems.item(i).fldIncomeDateStart.access = strAccess;
			vItems.item(i).fldIncomeDateEnd.access = strAccess;
			vItems.item(i).chbNoExpectedEnd.access = strAccess;
			vItems.item(i).fldNumMonths.access = strAccess;
			
			//If last, close other field type		
			if ( i == (vItems.length - 1) )
				vItems.item(i).fldTypeOther.access = strAccess;		
		}
	}
}
</script>
                           </event>
