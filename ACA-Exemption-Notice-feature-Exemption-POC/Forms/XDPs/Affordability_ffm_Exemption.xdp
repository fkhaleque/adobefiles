/**
 * Sets the presence of the Remove button in Jobs section
 *  @param - objJobs - jobs section object
 *  @param - intIndexSkip - index count to skip (-1 does not skip any index)
 */
function toggleRemoveJobBttn(objJobs, intIndexSkip)
{
	var intTotJobCount = objJobs.sfJob.instanceManager.count;
	var objNode = null;
	
	if ( intTotJobCount == 1 )
	{
		//Hide Remove button on single instance
		objJobs.resolveNode("sfJob[0]").sfButtons.bttnRemoveJob.presence = "hidden";
	}
	else if ( intTotJobCount == 2 &amp;&amp; intIndexSkip != -1 )
	{
		//Where only one instance would be remaining after removal,
		//hide Remove button on remianing instance prior to removing the other
		if ( intIndexSkip == 0 )
