/******************************************************************
 *
 * Load / Save bound data from field parts - Income
 *  
 ******************************************************************/

function loadIncomeBoundData(objParentDataNode, objParentFormNode)
{
	var intNodeLen = objParentDataNode.nodes.length;
	var vItems = objParentFormNode.resolveNodes("sfIncome[*]");

	//Note: Child node indexes are zero-based, and there is always initally one node in the data DOM
	var intIndex = intNodeLen - 3;   //Subtract 2 additional for no_other_income and other_income_other node values

	for ( var i=0; i&lt;=intIndex; i++ )
	{	
		var strChildNode = "income[" + i + "]";
		var objChildDataNode = objParentDataNode.resolveNode(strChildNode);	

		for( j=0; j&lt;vItems.length; j++ )
		{
			try
			{		
				//If exemption specified, then process
				if ( objChildDataNode.type.value == j+1 )
				{
					vItems.item(j).chbType.rawValue = "1";
					try { vItems.item(j).fldAmount.rawValue = objChildDataNode.other_income_amt.value; }  catch (e) { }
					try { vItems.item(j).lstHowOften.rawValue = objChildDataNode.other_income_frequency.value; }  catch (e) { }
					try { vItems.item(j).fldIncomeDateStart.rawValue = objChildDataNode.start_date.value; }  catch (e) { }
					try { vItems.item(j).fldIncomeDateEnd.rawValue = objChildDataNode.end_date.value; }  catch (e) { }		
					try { vItems.item(j).chbNoExpectedEnd.rawValue = objChildDataNode.ongoing.value; }  catch (e) { }
					try { vItems.item(j).fldNumMonths.rawValue = objChildDataNode.expected_months.value; }  catch (e) { }
																
					break;
				}
			}
			catch (e) { }
		}
	}

}


function saveIncomeBoundData(objParentDataNode, objParentFormNode)
{
	//Clear data DOM for parent node
	clearIncomeDataNodes(objParentDataNode);

	var vItems = objParentFormNode.resolveNodes("sfIncome[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		try
		{
			//If exemption selected, then process
			if ( vItems.item(i).chbType.rawValue != null &amp;&amp; vItems.item(i).chbType.rawValue &gt; 0 )
			{	
				var objChildDataNode = getIncomeDataNodeObject(objParentDataNode);
				//app.alert("objChildDataNode: " + objChildDataNode.name + " | objChildDataNode.index: " + objChildDataNode.index);

				//Save type ID
				var intId = i + 1;
				objChildDataNode.type.value = intId.toString();
			
				//Save other income amount
				if ( vItems.item(i).fldAmount.rawValue != null )
					objChildDataNode.other_income_amt.value = vItems.item(i).fldAmount.rawValue.toString();
				else
					objChildDataNode.other_income_amt.value = "";
					
				//Save frequency				
				if ( vItems.item(i).lstHowOften.rawValue != null )
					objChildDataNode.other_income_frequency.value = vItems.item(i).lstHowOften.rawValue;
				else
					objChildDataNode.other_income_frequency.value = "";

				//Save other income date started				
				if ( vItems.item(i).fldIncomeDateStart.rawValue != null )
					objChildDataNode.start_date.value = soValidations.getDateIsoFormat(vItems.item(i).fldIncomeDateStart);
				else
					objChildDataNode.start_date.value = "";
					
				//Save other income date ended				
				if ( vItems.item(i).fldIncomeDateEnd.rawValue != null )
					objChildDataNode.end_date.value = soValidations.getDateIsoFormat(vItems.item(i).fldIncomeDateEnd);
				else
					objChildDataNode.end_date.value = "";
					
				//Save no expected end date				
				if ( vItems.item(i).chbNoExpectedEnd.rawValue != null )
					objChildDataNode.ongoing.value = vItems.item(i).chbNoExpectedEnd.rawValue;
				else
					objChildDataNode.ongoing.value = "";

				//Save expected number of months					
				if ( vItems.item(i).fldNumMonths.rawValue != null )
					objChildDataNode.expected_months.value = vItems.item(i).fldNumMonths.rawValue.toString();
				else
					objChildDataNode.expected_months.value = "";				
			}
		}
		catch (e) {}
	}
}


/**
 * Returns (next) income data object from data DOM in which to load income info
 *  @param - objParentDataNode - Node to place data in DOM
 *  @return - new new data object from data DOM
 */
function getIncomeDataNodeObject(objParentDataNode)
{
	var intNodeLen = objParentDataNode.nodes.length;

	//Note: Child node indexes are zero-based, and there is always initally one node in the data DOM
	var intIndex = intNodeLen - 3;   //Subtract 2 additional for no_other_income and other_income_other node values
	var strChildNode = "income[" + intIndex + "]";
	var objChildDataNode = objParentDataNode.resolveNode(strChildNode);
	
	if ( objChildDataNode.type.value != "" &amp;&amp; objChildDataNode.type.value != null )
	{
		//Clone another data group node
		objParentDataNode.nodes.append( objParentDataNode.income.clone(1) );

		//Get data object for new clone
		intIndex++;
		strChildNode = "income[" + intIndex + "]";
		objChildDataNode = objParentDataNode.resolveNode(strChildNode);
	}

	return objChildDataNode;
		
}


/**
 * Clears income info in data node
 *  @param - objParentDataNode - Node to remove data in DOM
 */
function clearIncomeDataNodes(objParentDataNode)
{
	var intNodeLen = objParentDataNode.nodes.length;
	
	//app.alert(intNodeLen);

	//Note: Child node indexes are zero-based, and there is always initally one node in the data DOM
	var intIndex = intNodeLen - 3;   //Subtract 2 additional for no_other_income and other_income_other node values

	for ( var i=intIndex; i&gt;=0; i-- )
	{
		var strChildNode = "income[" + i + "]";
		var objChildDataNode = objParentDataNode.resolveNode(strChildNode);
		
		if ( i == 0 )
		{
			//clear data values if only one child node - or create value nodes for those that are missing from AF
			try { objChildDataNode.type.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "type");
						objChildDataNode.nodes.append(oValueNode);	}
						
			try { objChildDataNode.other_income_amt.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "other_income_amt");
						objChildDataNode.nodes.append(oValueNode);	}
						
			try { objChildDataNode.other_income_frequency.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "other_income_frequency");
						objChildDataNode.nodes.append(oValueNode);	}
						
			try { objChildDataNode.start_date.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "start_date");
						objChildDataNode.nodes.append(oValueNode);	}
						
			try { objChildDataNode.end_date.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "end_date");
						objChildDataNode.nodes.append(oValueNode);	}
			
			try { objChildDataNode.ongoing.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "ongoing");
						objChildDataNode.nodes.append(oValueNode);	}
						
			try { objChildDataNode.expected_months.value = ""; }
			catch (e) { var oValueNode = xfa.datasets.createNode("dataValue", "expected_months");
						objChildDataNode.nodes.append(oValueNode);	}
		}
		else
			try { objParentDataNode.nodes.remove(objChildDataNode); } catch (e) {};
	}
}


