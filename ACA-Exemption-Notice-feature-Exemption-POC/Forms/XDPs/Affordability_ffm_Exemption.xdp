                     <event activity="ready" ref="$layout" name="event__layout_ready">
                        <script contentType="application/x-javascript">
//Populate barcode when rendered on server
if ( soBarcoding.isOnServer() &amp;&amp; soBarcoding.isLayedOut() )
{
	// Concatenate Multi Fields
		
	var strDOB = sfPerson.sfName.fldDOBMonth.formattedValue
				+ sfPerson.sfName.fldDOBDay.formattedValue
				+ sfPerson.sfName.fldDOBYear.formattedValue;
				
	var strSSN = sfPerson.sfSSN.fldSSN1.formattedValue
				+ sfPerson.sfSSN.fldSSN2.formattedValue
				+ sfPerson.sfSSN.fldSSN3.formattedValue;

		var intPage = xfa.layout.page(this) - xfa.resolveNodes("data.#pageSet.PageX[*]").length - 1;
				
		var strPageData = soBarcoding.encodePageMeta( soExemption.getSeqAlphaNum(parseInt(Person2StartPg.value), intPage) )
		+ soBarcoding.encodeString(sfPerson.sfName.fldFirstName.rawValue)
		+ soBarcoding.encodeString(sfPerson.sfName.fldMiddleName.rawValue)
		+ soBarcoding.encodeString(sfPerson.sfName.fldLastName.rawValue)
		+ soBarcoding.encodeString(sfPerson.sfName.fldSuffix.rawValue)
		+ soBarcoding.encodeString(strDOB)
		+ soBarcoding.encodeString(sfPerson.sfName.rdoSex.rawValue)
		+ soBarcoding.encodeString(strSSN)													
		+ soBarcoding.encodeString(sfTaxes.rdoPlanToFile.rawValue)
		+ soBarcoding.encodeString(sfTaxes.rdoFileJointly.rawValue)
		+ soBarcoding.encodeString(sfTaxes.fldSpouse.rawValue)
		+ soBarcoding.encodeString(sfTaxes.rdoClaimdependents.rawValue)
		+ soBarcoding.encodeString(sfTaxes.flddependents.rawValue)
		
		//Questions specific to person2s
		+ soBarcoding.encodeString(sfTaxes.rdoOnP1Taxes.rawValue)	
		+ soBarcoding.encodeString(sfTaxes.fldFiler.rawValue)
		+ soBarcoding.encodeString(sfTaxes.fldRelToFiler.rawValue)		
		
		+ soBarcoding.encodeString(sfWantExemption.rdoWantExemption.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect1.rdoPregnant.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect1.fldNumberofBabies.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect2.rdoLivewithChild.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect3.rdoFulltimeStudent.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect4.rdoFosterCare.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect5.rdoUseTobacco.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect6.rdoUSCitizen.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect7.rdoDerivedCitizen.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect8.fldAlienNumber.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect8.fldCertifgicateNumber.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect9.chkEligibleImmStatusYes.rawValue)	
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect10.fldImmigrationDocument.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect10.fldStatusType.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect10.fldImmDocName.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect11.fldAliennumber.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect11.fldPassportNumber.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect12.fldSEVISID.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect12.fldOther.rawValue)								
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect13.rdoLivedinUS.rawValue)
		+ soBarcoding.encodeString(sfMiscQuestions.sfQuestSect13.rdoActiveMilitary.rawValue)
		+ soBarcoding.encodeEthnicity(sfEthnicityAndRace.sfEthnicities)
		+ soBarcoding.encodeRace(sfEthnicityAndRace.sfRaces);
	
	//console.println(strPageData);	
	
	this.rawValue = strPageData;
	
	//Show barcode
	this.presence = "visible";	
}
</script>
