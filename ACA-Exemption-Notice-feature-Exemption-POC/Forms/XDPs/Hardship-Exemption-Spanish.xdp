function toggleHardshipOngoing(fldOngoing)
{
	if ( fldOngoing.rawValue != null &amp;&amp; fldOngoing.rawValue == "Y" )
	{
		fldOngoing.parent.fldHSDateEnd.rawValue = null;
		fldOngoing.parent.fldHSDateEnd.access = "readOnly";	
	}
	else
		fldOngoing.parent.fldHSDateEnd.access = "open";	
}


function toggleHardship(fldCheck)
{
	var access="open";
	
	if ( fldCheck.rawValue == 0 )
	{
		fldCheck.parent.fldHSTaxYear.rawValue = null;
		fldCheck.parent.fldHSDateStart.rawValue = null;
		fldCheck.parent.fldHSDateEnd.rawValue = null;
		fldCheck.parent.fldHSOngoing.rawValue = null;
		access="readOnly";
	}
	
	fldCheck.parent.fldHSTaxYear.access=access;
	fldCheck.parent.fldHSDateStart.access=access;
	fldCheck.parent.fldHSDateEnd.access=access;
	fldCheck.parent.fldHSOngoing.access=access;
}



function clearAllHardships(sfHardships)
{
	// If a user selects "NO" to hardships, then clear all the grid.
	var vItems = sfHardships.sfHardship.all;
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		// clear value
		vItems.item(i).fldHS.rawValue = 0;
		vItems.item(i).fldHSTaxYear.rawValue = null;
		vItems.item(i).fldHSDateStart.rawValue = null;
		vItems.item(i).fldHSDateEnd.rawValue = null;
		vItems.item(i).fldHSOngoing.rawValue = null;
	}
}


function initializeHardshipGrid(sfHardships)
{
	var vItems = sfHardships.resolveNodes("sfHardship[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		//console.println("initHardGrid: " + vItems.item(i).name + " " + i);
		toggleHardship(vItems.item(i).fldHS);
	}
}


function generateHardshipBarcodeString(sfHardships)
{
	var hardshipBarcode = "";
	var hardshipsSelected = 0;
	var vItems = sfHardships.sfHardship.all;
	
	//console.println("vItems.length: " + vItems.length);

	for( i=0; i&lt;vItems.length; i++ )
	{
		var objNode = sfHardships.resolveNode("sfHardship[" + i + "]");
		//console.println("objNode: " + objNode.somExpression);
		
		var flgHS = objNode.fldHS.rawValue;
		
		if ( flgHS == "1" )
		{
			//Append HS ID first - return value from encodeNumber containing ";" introdues bug in FOR loop (JS issue)
			hardshipBarcode += soBarcoding.encodeNumber(i + 1, true, 2);
			
			hardshipBarcode += ( "20" + soBarcoding.encodeString(objNode.fldHSTaxYear.rawValue) )
							+ soBarcoding.encodeDate( soValidations.getDateIsoFormat(objNode.fldHSDateStart) )
							+ soBarcoding.encodeDate( soValidations.getDateIsoFormat(objNode.fldHSDateEnd) )
							+ soBarcoding.encodeString(objNode.fldHSOngoing.rawValue);
			
			hardshipsSelected++;
			
			//console.println("hardshipBarcode: " + hardshipBarcode);
		}
	}
	
	//Per requirements, a max of 5 hardships is to be assumed
	for ( i=hardshipsSelected; i&lt;5; i++ )
	{
		//Pad-out data with empty separators
		hardshipBarcode += ";;;;;";
	}
	
	return hardshipBarcode;
}


function validateHardshipStartDate(oFldDate)
{
	if ( oFldDate.rawValue != "" &amp;&amp; oFldDate.rawValue != null )
	{
		var arrDate = new Array();

		arrDate = HardshipStartDt.value.split("/");    //HardshipStartDt is a form variable of MM/DD/YYYY
		var oHardshipStartDt = new Date ( arrDate[2], arrDate[0] - 1, arrDate[1] );
		//app.alert("Hardship Start Date: " + util.printd("mm/dd/yyyy", oHardshipStartDt));
	
		arrDate = [];
		arrDate = oFldDate.formattedValue.split("/"); 
		var oFldDt = new Date ( arrDate[2], arrDate[0] - 1, arrDate[1] );
		//app.alert("Field Date: " + util.printd("mm/dd/yyyy", oFldDt))	
	
		if ( oHardshipStartDt != null &amp;&amp; oFldDt &lt; oHardshipStartDt )
		{
			xfa.host.messageBox("If your hardship started before " + HardshipStartDt.value + ", just list the first date\n" + 
									"people were required either to get health coverage or qualify for an exemption.", soExemption.FORM_NAME,1,0);
			return false;
		}
	}
	
	return true;
}


function validateHardshipEndDate(oFldStartDate, oFldEndDate)
{
	if ( oFldStartDate.rawValue != "" &amp;&amp; oFldStartDate.rawValue != null &amp;&amp;
			oFldEndDate.rawValue != "" &amp;&amp; oFldEndDate.rawValue != null )
	{
		var arrDate = new Array();
		
		arrDate = oFldStartDate.formattedValue.split("/");
		var oHardshipStartDt = new Date ( arrDate[2], arrDate[0] - 1, arrDate[1] );
		//app.alert("Hardship Start Date: " + util.printd("mm/dd/yyyy", oHardshipStartDt));

		arrDate = [];
		arrDate = oFldEndDate.formattedValue.split("/");
		var oHardshipEndDt = new Date ( arrDate[2], arrDate[0] - 1, arrDate[1] );
		//app.alert("Hardship End Date: " + util.printd("mm/dd/yyyy", oHardshipEndDt))	

		if ( ( oHardshipStartDt != null &amp;&amp; oHardshipEndDt != null ) &amp;&amp; oHardshipEndDt &lt; oHardshipStartDt )
		{
			xfa.host.messageBox("Date hardship ended cannot be before date hardship started.", soExemption.FORM_NAME,1,0);
			oFldEndDate.rawValue = "";
			xfa.host.setFocus(oFldEndDate);
			return false;
		}
	}

	return true;
}


function validateHardshipStartYear(oFldYrEntry)
{
	if ( oFldYrEntry.rawValue != "" &amp;&amp; oFldYrEntry.rawValue != null )
	{
		sFldYrEntry = "20" + oFldYrEntry.rawValue;
		
		if (  parseInt(sFldYrEntry) &lt; parseInt(HardshipStartYr.value) )   //HardshipStartYear is a form variable of YYYY
		{
			xfa.host.messageBox("The tax year cannot be earlier than " + HardshipStartYr.value + ".", soExemption.FORM_NAME,1,0);
			oFldYrEntry.rawValue = "";
			xfa.host.setFocus(oFldYrEntry);
			return false;
		}
	}
	
	return true;		
}


function validateHardshipDatesIncludeTaxYear(oTaxYear, oFldStartDate, oFldEndDate)
{
	if ( oTaxYear.rawValue != "" &amp;&amp; oTaxYear.rawValue != null &amp;&amp;
			oFldStartDate.rawValue != "" &amp;&amp; oFldStartDate.rawValue != null )
	{
		var arrDate = new Array();
		
		arrDate = oFldStartDate.formattedValue.split("/");
		var strYearStartDt = arrDate[2].substr(2,2);
		//console.println("Hardship Start Date: " + strYearStartDt);

		if ( oFldEndDate.rawValue != "" &amp;&amp; oFldEndDate.rawValue != null )
		{		 
			arrDate = [];
			arrDate = oFldEndDate.formattedValue.split("/");
			var strYearEndDt = arrDate[2].substr(2,2);
			//console.println("Hardship End Date Year: " + strYearEndDt);

			if ( strYearStartDt != oTaxYear.rawValue &amp;&amp; strYearEndDt != oTaxYear.rawValue )
			{
				return false;
			}
		}
		else
		{
			if ( strYearStartDt != oTaxYear.rawValue )
			{
				return false;
			}
		}	
	}

	return true;
}


function setHardshipDocReminders(arrHardships)
{
	/*
	 * arrHardships[0] = Person 1
	 * arrHardships[1] = Person 2
	 * arrHardships[2] = Person 3
	 * etc.
	 */
	
	if ( arrHardships.length &gt; 0 )
	{	
		for( var i=0; i&lt;arrHardships.length; i++ )
		{			
			if ( arrHardships[i] != "" )
			{		
				var objPage = null;
				var objSubform = null;
			
				//If pgSupportingDocs page is still hidden on initialize, then make visible
				//Get initial sfSupportingDocs instance or add one as necessary				
				objPage = xfa.resolveNode("pgSupportingDocs");
				
				if ( objPage.presence == "hidden" )
				{
					objPage.presence = "visible";
					objSubform = xfa.resolveNode("pgSupportingDocs.sfFlowed.sfSupportingDocs[0]");				
				}	
				else
				{
					//app.alert("adding instance");	
					objSubform = pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.addInstance(1);									
				}		
						
				//app.alert("objSubform: " + objSubform.name + " | Index: " + objSubform.index);

				//Initialize person number and name of applicant
				if ( i == 0 )
				{
					objSubform.sfFormHeader.fldPersonX.rawValue = "Person 1 - Required documents for:";
					
					objSubform.sfFormHeader.fldPersonName.rawValue = assembleHSPersonName(pgStep2Person1.sfPerson1.sfPerson.sfName.fldFirstName, 
																							pgStep2Person1.sfPerson1.sfPerson.sfName.fldMiddleName, 
																								pgStep2Person1.sfPerson1.sfPerson.sfName.fldLastName, 
																									pgStep2Person1.sfPerson1.sfPerson.sfName.fldSuffix);
				}
				else
				{
					objSubform.sfFormHeader.fldPersonX.rawValue = "Person " + (i + 1) + " - Required documents for:";
								
					objSubform.sfFormHeader.fldPersonName.rawValue = assembleHSPersonName(pgStep2Person2.sfFlowed.resolveNode("sfPerson2s[" + (i - 1) + "]").sfPerson.sfName.fldFirstName,
																							pgStep2Person2.sfFlowed.resolveNode("sfPerson2s[" + (i - 1) + "]").sfPerson.sfName.fldMiddleName, 
																								pgStep2Person2.sfFlowed.resolveNode("sfPerson2s[" + (i - 1) + "]").sfPerson.sfName.fldLastName, 
																									pgStep2Person2.sfFlowed.resolveNode("sfPerson2s[" + (i - 1) + "]").sfPerson.sfName.fldSuffix);			
				}
					
				//Parse list of checkboxes and set visible or hidden
				var intHSCheckbox = objSubform.sfContent.sfHardships.resolveNodes("sfHardship[*]");
				//app.alert("intHSCheckbox : " + intHSCheckbox.length);
							
				for ( var j=0; j&lt;intHSCheckbox.length; j++ )
				{
					var strID = soValidations.padDigits(j + 1, 2);
					var intPos = arrHardships[i].indexOf(strID); 
					
					//Hide all checkbox subforms not found in string of IDs
					if ( intPos == -1 )
					{
						//console.println("Person " + (i + 1) + " | Hiding HS : " + soValidations.padDigits(j + 1, 2));					
						var objHSField = objSubform.sfContent.sfHardships.resolveNode("sfHardship[" + j + "]");		
						objHSField.presence = "hidden";
						objHSField.chkHS.rawValue = "0";											
					}
				}
			}
		}
	}
}


function collectHardshipDocReminders()
{
	var objPage = xfa.resolveNode("pgStep2Person2");
	var arrHardships = new Array();	

	//Collect hardships Step 2 - First person
	arrHardships[0] = getHSDocIDsForPerson(xfa.resolveNode("pgStep2Person1.sfPerson1[0]"));

	//If pgStep2Person2 is visible, then collect selectd hardships
	if ( objPage.presence != "hidden" )
	{
		//Collect hardships  Step 2 - All Person 2s
		var intStep2PeopleCount = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s").instanceManager.count;
			
		if ( intStep2PeopleCount &gt; 0 )
		{	
			var intArrCount = 1;
			
			for( var i=0; i&lt;intStep2PeopleCount; i++ )
			{
				objNode = xfa.resolveNode("pgStep2Person2.sfFlowed.sfPerson2s[" + i + "]");
				arrHardships[intArrCount] = getHSDocIDsForPerson(objNode);
				
				intArrCount++;
			}
		}
	}

	return arrHardships;
}


function resetHardshipDocReminders()
{		
	var objPage = xfa.resolveNode("pgSupportingDocs");	
				
	if ( objPage.presence != "hidden" )
	{
		//Hide pgSupportingDocs page
		objPage.presence = "hidden";		
		
		var intInstCount = pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.count;	
		
		//Remove sfSupportingDocs instances, but keep one (zero based)
		if ( intInstCount &gt; 1 )
		{
			for ( var i=intInstCount-1; i&gt;0; i-- )
			{
				pgSupportingDocs.sfFlowed.sfSupportingDocs.instanceManager.removeInstance(i);			
				//console.println("removing: " + i);		
			}
		}	

		//Get remaining sfSupportingDocs instance
		var objSubform = xfa.resolveNode("pgSupportingDocs.sfFlowed.sfSupportingDocs[0]");

		//For last instance, clear description and name fields
		objSubform.sfFormHeader.fldPersonX.rawValue = "";
		objSubform.sfFormHeader.fldPersonName.rawValue = "";

		//For last instance, make visible and clear all checkboxes
		var intHSCheckbox = objSubform.sfContent.sfHardships.resolveNodes("sfHardship[*]");
		
		for ( var j=0; j&lt;intHSCheckbox.length; j++ )
		{
			//console.println("Resetting HS : " + soValidations.padDigits(j + 1, 2));
			var objHSField = objSubform.sfContent.sfHardships.resolveNode("sfHardship[" + j + "]");		
			objHSField.presence = "visible";
			objHSField.chkHS.rawValue = "0";					
		}
	}	
}	


function getHSDocIDsForPerson(objNode)
{
	var strDocReminderIDs = "";
	var vItems = objNode.sfHardships.resolveNodes("sfHardship[*]");
	
	for( i=0; i&lt;vItems.length; i++ )
	{
		// Wants the exemption?
		if ( vItems.item(i).fldHS.rawValue &gt; 0 )
		{	
			if ( strDocReminderIDs == "" )
				strDocReminderIDs = fetchHSDocID(i);	
			else
				strDocReminderIDs += "," + fetchHSDocID(i);
		}
	}
	
	return strDocReminderIDs;
	
}


function fetchHSDocID(intHardshipId)
{
	var	strDocID = "";
	var strDocDesc = "";

	switch (intHardshipId)
	{
		case 0:  // You were homeless
		strDocID = "";  //SKIP - no documents required
		strDocDesc = "";		
		break;
	
		case 1:  // You were evicted or were facing eviction or foreclosure
		strDocID = "02";		
		strDocDesc = "- Eviction or foreclosure notice";
		break;
	
		case 2:  // You received a shut-off notice from a utility company
		strDocID = "03";		
		strDocDesc = "- Shut-off notice from a utility company which states service has or will be shut-off";
		break;
	
		case 3:  // You recently experienced domestic violenced
		strDocID = "";  //SKIP - no documents require
		strDocDesc = "";		
		break;
	
		case 4:  // You recently experienced the death of a close family member
		strDocID = "05";		
		strDocDesc = "- Death certificate, death notice from newspaper, funeral service program, funeral expenses, coroner's report, military notification of death, or other official notice of death";
		break;
	
		case 5:  // You experienced a fire, flood, or other natural or human-caused disaster that caused substantial damage to your property
		strDocID = "06";		
		strDocDesc = "- Police or fire report, insurance claim, or other document from government agency, private entity, or news source about the event";
		break;
	
		case 6:  // You filed for bankruptcy
		strDocID = "07";		
		strDocDesc = "- Official bankruptcy filing documents";
		break;
	
		case 7:  // You had medical expenses you couldn't pay
		strDocID = "08";		
		strDocDesc = "- Medical bills";
		break;
	
		case 8:  // You experienced unexpected increases in necessary expenses due to caring for an ill, disabled, or aging family member
		strDocID = "09";		
		strDocDesc = "- Receipts for bills or services related to care, like medical bills, home care services, or transportation receipts";
		break;
	
		case 9:  // A child you expect to claim as a tax dependent has been denied coverage in Medicaid and the Children's Health Insurance Program (CHIP), and another person is required by court order to give medical support to the child
		strDocID = "10";		
		strDocDesc = "- Court order that covers the time period for which you want the exemption for the child AND copies of eligibility notices for Medicaid and CHIP which shows that the child has been denied coverage";
		break;
	
		case 10:  // As a result of a Health Insurance Marketplace appeals decision, you're eligible either for: 1) enrolment in a qualified health plan(QHP) through the Marketplace, 2) lower costs on your monthly premiums, or 3) cost-sharing reductions for a time period when you weren't enrolled in a QHP Marketplace
		strDocID = "11";		
		strDocDesc = "- Health Insurance Marketplace notice of appeals decision. The decision must show the names of everyone in your tax household that wants this exemption";
		break;
	
		case 11:  // You were determined ineligible for Medicaid because your state didn't expand eligibility for Medicaid under the Affordable Care Act
		strDocID = "12";		
		strDocDesc = "- Notice of denial of eligibility for Medicaid. The notice must be from a date during the time period for which you're requesting the exemption";
		break;
	
		case 12:  // You received a notice saying that your current health insurance plan purchased on the individual market (non-group coverage) will be cancelled because it does not meet Affordable Care Act requirements, and you consider other available plans unaffordable 
		strDocID = "13";		
		strDocDesc = "- Notice of cancellation from the insurance company. The notice must show the names of everyone in your tax household that wants this exemption";
		break;
	
		case 13:  // You experienced a hardship that kept you from getting health insurance that's NOT listed in categories #1-13
		strDocID = "14";		
		strDocDesc = "- There are a limited number of other hardships that qualify. Go to Healthcare.gov/fees-exemptions/hardship-exemptions/ to see this list";
		break;
	
	}
	
	return strDocID;
	
}


function assembleHSPersonName(objFirstNm, objMiddleNm, objLastNm, objSuffixNm)
{
	var strFullName = "";
	
	if ( objFirstNm.rawValue != "" &amp;&amp; objFirstNm.rawValue != null )
		strFullName += objFirstNm.rawValue;
	
	if ( objMiddleNm.rawValue != "" &amp;&amp; objMiddleNm.rawValue != null )
		strFullName += " " + objMiddleNm.rawValue;
		
	if ( objLastNm.rawValue != "" &amp;&amp; objLastNm.rawValue != null )
		strFullName += " " + objLastNm.rawValue;		

	if ( objSuffixNm.rawValue != "" &amp;&amp; objSuffixNm.rawValue != null )
		strFullName += ", " + objSuffixNm.rawValue;	
	
	return soValidations.trimString(strFullName);
}


