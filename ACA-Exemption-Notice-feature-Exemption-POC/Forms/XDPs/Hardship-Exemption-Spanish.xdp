			strPageData += soBarcoding.encodeNumber(i+1, true, 2) 
						+ soBarcoding.encodeString(objHSField.chkHS.rawValue);
		
			hardshipsSelected++;
		}
	}
	
	//Per requirements, a max of 5 hardships is to be assumed
	for ( i=hardshipsSelected; i&lt;5; i++ )
	{
		//Pad-out data with empty separators
		strPageData += ";;";
	}
	
	this.rawValue = strPageData;
	
	//Show barcode
	this.presence = "visible";
}
</script>
