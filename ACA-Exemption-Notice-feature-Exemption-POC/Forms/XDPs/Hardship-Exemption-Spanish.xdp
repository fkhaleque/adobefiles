      <event activity="prePrint" ref="$host" name="event__prePrint">
         <script contentType="application/x-javascript" runAt="both">
if ( soExemption.PERFORM_SECTIONAL_VALIDATION )
{
	soValidations.resetErrors();
		
	if ( soValidations.validateForm() )
	{
		//Validaate Appendix C if there is data
		if ( !soExemption.validateAppendixC_CounselorAssist(xfa.resolveNode("pgAppendixC[0]")) )  //Validate Appendix C agent, nav, counselor and broker assistance section of form
		{
			if ( soExemption.PERFORM_SECTIONAL_VALIDATION_ENFORCE_FILL ) 
			{
				// not correct
				xfa.host.messageBox("Please complete the form in APPENDIX C - Counselor Assistance section.\n\n" + soValidations.getErrorMessage(), soExemption.FORM_NAME, 1,0);
				soValidations.highlightFirstErrorField();
			}
		}
		else if ( !soExemption.validateAppendixC_AuthorizedRepAssist(xfa.resolveNode("pgAppendixC[0]")) )  	//Validate Appendix C authorized representative section of form
		{
			if ( soExemption.PERFORM_SECTIONAL_VALIDATION_ENFORCE_FILL ) 
			{
				// not correct
				xfa.host.messageBox("Please complete the form in APPENDIX C - Authorized Representative section.\n\n" + soValidations.getErrorMessage(), soExemption.FORM_NAME, 1,0);
				soValidations.highlightFirstErrorField();
			}
		}
	
		//Check if form is still valid
		if ( soValidations.isValid() )
			xfa.host.messageBox("Application is verified.\n\nPlease remember to sign your application and include any required supporting documents when mailing.\n\n" +
									"Please note that the applicant may receive a phone call from the Marketplace about this application.", soExemption.FORM_NAME,1,0);
	}
	else
	{
		xfa.event.cancelAction = true;
		soValidations.errorMessageBox();
		soValidations.highlightFirstErrorField();
	}
}



</script>
      </event>
