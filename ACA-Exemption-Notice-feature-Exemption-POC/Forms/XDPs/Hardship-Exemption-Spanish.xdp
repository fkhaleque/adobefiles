/**
 * Checks zip code based on validation rules
 * and displays message and clears field if not valid
 *  @param - oFldZip - zip code field object
 *  @return - returns true if zip code valid otherwise false
 */
function checkZipcode(oFldZip)
{
	if ( oFldZip.rawValue != "" &amp;&amp; oFldZip.rawValue != null )
	{
		if ( isValidZIP(oFldZip.rawValue) )
		{
			return checkZipcodeForDups(oFldZip);
		}
		else
		{
			xfa.host.messageBox("'" + oFldZip.rawValue + "' is not a valid zip code.", soExemption.FORM_NAME,1,0);
			oFldZip.rawValue = "";
			xfa.host.setFocus(oFldZip);	
			return false;
		}
	}
}


/**
 * Checks for duplicate repeating sequence number in zip code
 * and displays message and clears field if not valid
 *  @param - oFldZip - zip code field object
 *  @return - returns true if zip code valid otherwise false
 */
function checkZipcodeForDups(oFldZip)
{
	if ( isValidZIP(oFldZip.rawValue) )
	{
		var arrZip = new Array();
		arrZip = oFldZip.rawValue.split("");
		
		var testVal = arrZip[0];
		var count = 0;
		
		for( var i=0; i&lt;arrZip.length; i++ )
		{
			if ( testVal == arrZip[i] )
				count++;
		}
			
		if ( count == 5 )
		{
			xfa.host.messageBox("'" + oFldZip.rawValue + "' for zip code cannot be a repeating set of numbers.", soExemption.FORM_NAME,1,0);
			oFldZip.rawValue = "";
			xfa.host.setFocus(oFldZip);	
			return false;
		}
	}
	
	return true;

}


/**
 * Check for duplicate repeating sequence number in phone
 *  @param - oPhnAreaCd - area code field object
 *  @param - oPhnPrefix - phone prefix field object
 *  @param - oPhnSuffix - phone suffix field object 
 *  @return - returns true if phone valid otherwise false
 */
function checkPhoneForDups(oPhnAreaCd, oPhnPrefix, oPhnSuffix)
{
	var phoneNumber = oPhnAreaCd.formattedValue + oPhnPrefix.formattedValue + oPhnSuffix.formattedValue;

	if ( isValidPhone(phoneNumber) )
	{
		var arrPhn = new Array();
		arrPhn = phoneNumber.split("");
		
		var testVal = arrPhn[0];
		var count = 0;
		
		for( var i=0; i&lt;arrPhn.length; i++ )
		{
			if ( testVal == arrPhn[i] )
				count++;
		}
			
		if ( count == 10 )
		{
			xfa.host.messageBox("'" + oPhnAreaCd.formattedValue + "-" + oPhnPrefix.formattedValue + "-" + oPhnSuffix.formattedValue + "' for phone number cannot be a repeating set of numbers.", soExemption.FORM_NAME,1,0);
			oPhnAreaCd.rawValue = "";
			oPhnPrefix.rawValue = "";
			oPhnSuffix.rawValue = "";	
			xfa.host.setFocus(oPhnAreaCd);				
			return false;
		}
	}
	
	return true;

}


