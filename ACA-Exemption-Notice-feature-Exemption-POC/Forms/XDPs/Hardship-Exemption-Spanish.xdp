	strData += ";";	

	var strDataOther = objSfRace.fldOtherRace.rawValue;
	
	if ( strDataOther != "" &amp;&amp; strDataOther != null )
	{
		//remove any delimeters within string	
		strDataOther.replace(/;/g,"");
		
		if ( strDataOther != "" )
		{		
			strData += strDataOther;
		}
	}
	
	return strData + ";";

}


/**
 * Encodes ethnicity data (first occurence only) for QR barcode
 *  @param - objSfEthnic - subform for ethnicity data
 *  @return - encoded string 
 */
function encodeEthnicity(objSfEthnic)
{
	var strData = "";
	
	//Find first occurence of selected ethnicity - there are currently 6 ethnicities in form
	for (i=0; i&lt;=5; i++)
	{
		var objSfField = objSfEthnic.resolveNode("sfEthnic[" + i + "]");
		
		if ( objSfField.chkEthnic.rawValue != "0" )
		{
			//strData += "," + objSfField.chkEthnic.resolveNode("caption.value.#text").value;
			strData += objSfField.chkEthnic.rawValue;
			break;		
		}
	}
	
	//Append delimiter whether there is data or not
	strData += ";";

	var strDataOther = objSfEthnic.fldOtherEthnicity.rawValue;
	
	if ( strDataOther != "" &amp;&amp; strDataOther != null )
	{
		//remove any delimeters within string	
		strDataOther.replace(/;/g,"");
		
		if ( strDataOther != "" )
		{		
			strData += strDataOther;
		}
	}
	
	return strData + ";";

}


/**
 * Encodes best time to call data (first occurence only) for QR barcode
 *  @param - objSfCallTime - subform for best time to call data
 *  @return - encoded string 
 */
function encodeBestCallTime(objSfCallTime)
{
	var strData = "";
	
	//Find first occurence best call time - there are currently 4 time options in form
	for (i=0; i&lt;=3; i++)
	{
		var objSfField = objSfCallTime.resolveNode("sfTime[" + i + "]");
		
		if ( objSfField.chkTime.rawValue != "0" )
		{
			//strData += "," + objSfField.chkTime.resolveNode("caption.value.#text").value;
			strData += objSfField.chkTime.rawValue;
			break;		
		}
	}
	
	return strData + ";";

}


/**
 * Shows string concoded in QR barcode
 *   @param - strData - string data generated for barcode
 */
function showQR(strData)
{
	if ( strData == null || strData == " " || strData == "" )
	{
		xfa.host.messageBox("Nothing to show yet.\n\nQR will populate prior to print.\n\nAttempt to print and try previewing again.");
	}
	else
	{
		xfa.host.messageBox("QR Code Data:\n\n" + strData);
	}
}
